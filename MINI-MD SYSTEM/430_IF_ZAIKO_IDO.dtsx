                <property
                  dataType="System.String"
                  description="実行する SQL コマンド。"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	'1000' AS CORP_CODE,
	'STOCK_MOVE' AS IF_SYSTEM_FUNC_ID,
	CONVERT(VARCHAR(8),移動伝票日付,112) + 出庫サイト + '01' AS IF_SYSTEM_NO,
	'' AS CORRECT_DIV,
	'' AS CORRECT_IF_SYSTEM_NO,
	CONVERT(VARCHAR(8),移動伝票日付,112) AS BOOK_DATE,
	'IF9999' AS BOOK_USER_ID,
	'IF9999' AS H_SECTION_CODE,
	'0' AS TERM_DIV,
	'10' AS SLIP_TYPE,
	'在庫移動' + CONVERT(VARCHAR(8),移動伝票日付,112) + RIGHT('00000' + 出庫サイト,5) HD_MEMO_TEXT,
	ROW_NUMBER() OVER(PARTITION BY 移動伝票日付,出庫サイト ORDER BY 移動伝票日付,出庫サイト) AS ROW_NO,
	'1' AS D_AND_C_DIV,
	RIGHT('00000' + 出庫サイト,5) AS SECTION_CODE,
	'8120500' AS ITEM_CODE,
	SUM(在庫移動原価) AS AMOUNT,
	'0' AS TAX_INOUT_DIV,
	'TC00' AS TAX_DIV_CODE,
	'1' AS TAX_INP_DIV,
	'0' AS TAX_FIX,
	'在庫移動' + RIGHT('00000' + 出庫サイト,5) + '→' +RIGHT('00000' + 入庫サイト,5) AS DTL_MEMO_TEXT,
	'' AS SUPPLIER_CODE,
	(-1) * SUM(在庫移動数) AS DTL_EXTRA_NUM1,
	(-1) * SUM(在庫移動原価) AS DTL_EXTRA_NUM2,
	'0' AS DTL_EXTRA_NUM3,
	'0' AS DTL_EXTRA_NUM4,
	'0' AS DTL_EXTRA_NUM5
FROM D_ZAIKO_IDO_DENPYOU_MEISAI
WHERE 移動伝票日付 BETWEEN ? AND ? 
AND 買取受託='0'
GROUP BY 移動伝票日付,出庫サイト,入庫サイト</property>
