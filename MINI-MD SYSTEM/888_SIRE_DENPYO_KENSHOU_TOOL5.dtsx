              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	ISNULL(MEISAI.仕入伝票日付,SHIRE.仕入日付) AS 伝票日付,
	ISNULL(MEISAI.実績数,0) AS 伝票点数,
	ISNULL(MEISAI.原価額,0) AS 伝票原価,
	ISNULL(MEISAI.実績数,0) - ISNULL(SHIRE.仕入数,0) AS 伝票点数差,
	ISNULL(MEISAI.原価額,0) - ISNULL(SHIRE.仕入原価,0) AS 伝票原価差,
	ISNULL(SHIRE.仕入数,0) AS 仕入数,
	ISNULL(SHIRE.仕入原価,0) AS 仕入原価
FROM (
	SELECT
		仕入伝票日付,
		SUM(実績数) AS 実績数,
		SUM(原価額) AS 原価額
	FROM D_SIIRE_DENPYOU_MEISAI
	WHERE 仕入伝票日付 BETWEEN ? AND ? 
	GROUP BY 仕入伝票日付) AS MEISAI
FULL JOIN (
		SELECT
			ISNULL(SIRE.仕入日付,HENPIN.返品日付) AS 仕入日付,
			ISNULL(SIRE.仕入数,0) + ISNULL(HENPIN.返品数,0) AS 仕入数,
			ISNULL(SIRE.仕入原価,0) + ISNULL(HENPIN.返品原価,0) AS 仕入原価
		FROM
			(
				SELECT 仕入日付,SUM(仕入数) AS 仕入数,
					SUM(仕入原価) AS 仕入原価,
					SUM(仕入売価) AS 仕入売価
				FROM T_DAY_SKU_SIIRE_JISSEKI
				WHERE 仕入日付 BETWEEN ? AND ? 
				GROUP BY 仕入日付
			) AS SIRE
			FULL JOIN 
			(
				SELECT 返品日付,SUM(返品数) AS 返品数,
					SUM(返品原価額) AS 返品原価,
					SUM(返品売価額) AS 返品売価
				FROM T_DAY_SKU_HENPIN_JISSEKI
				WHERE 返品日付 BETWEEN ? AND ? 
				GROUP BY 返品日付
			) AS HENPIN
		ON HENPIN.返品日付 = SIRE.仕入日付
		) AS SHIRE
ON MEISAI.仕入伝票日付 = SHIRE.仕入日付
ORDER BY MEISAI.仕入伝票日付,SHIRE.仕入日付</property>
