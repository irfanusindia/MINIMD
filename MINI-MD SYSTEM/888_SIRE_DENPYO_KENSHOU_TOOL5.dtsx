                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	ISNULL(MEISAI.仕入伝票日付,HDR.仕入伝票日付) AS 伝票日付,
	ISNULL(MEISAI.実績数,0) AS 伝票点数,
	ISNULL(MEISAI.原価額,0) AS 伝票原価,
	ISNULL(MEISAI.実績数,0) - ISNULL(HDR.実績数,0) AS 伝票点数差,
	ISNULL(MEISAI.原価額,0) - ISNULL(HDR.原価額,0) AS 伝票原価差,
	ISNULL(HDR.実績数,0) AS 仕入数,
	ISNULL(HDR.原価額,0) AS 仕入原価
FROM (
		SELECT
			仕入伝票日付,
			SUM(実績数) AS 実績数,
			SUM(原価額) AS 原価額
		FROM D_SIIRE_DENPYOU_MEISAI
		WHERE 仕入伝票日付 BETWEEN ? AND ? 
		GROUP BY 仕入伝票日付
	) AS MEISAI
FULL JOIN (
		SELECT
			仕入伝票日付,
			SUM(実績数) AS 実績数,
			SUM(原価額) AS 原価額
		FROM D_SIIRE_DENPYOU_HEADER
		WHERE 仕入伝票日付 BETWEEN ? AND ? 
		GROUP BY 仕入伝票日付
		) AS HDR
ON MEISAI.仕入伝票日付 = HDR.仕入伝票日付
ORDER BY MEISAI.仕入伝票日付,HDR.仕入伝票日付</property>
