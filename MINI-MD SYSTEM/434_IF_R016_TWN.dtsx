    '' AS [プラス/マイナス_2],
    売上売価 AS 売上金額,
    '' AS [条件のプラス/マイナス_1],
    CASE WHEN 売上割引 &gt; 0 THEN 'ZB00' ELSE '' END AS [条件/割引タイプ_2],
    売上割引 AS 販売割引,
    '' AS 条件タイプID,
    '' AS [20文字式条件タイプID],
    '' AS 項目CONDIDの識別子,
    '' AS ボーナスバイテキスト,
    '' AS 税コード,
    '' AS 税額_1,
    '' AS [条件のプラス/マイナス],
    '' AS 税タイプ,
    '' AS 税額_2,
    '' AS 項目グループ,
    '' AS 項目名,
    '' AS 項目値
FROM D_URIAGE_DENPYOU_MEISAI
WHERE 発生日 BETWEEN ? AND ? 
AND 店舗コード='8011'</property>
