              <properties>
                <property
                  dataType="System.Int32"
                  description="コマンドがタイムアウトするまでの秒数。値 0 はタイムアウトしないことを表します。"
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="行セットを開くために使用するデータベース オブジェクトの名前を指定します。"
