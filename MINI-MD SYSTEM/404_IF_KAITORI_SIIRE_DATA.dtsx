                <property
                  dataType="System.String"
                  description="行セットを開くために使用するデータベース オブジェクトの名前を含む変数を指定します。"
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="実行する SQL コマンド。"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
  [Y_DO],[M_DO],[W_DO],[BMN_CD],
  [J_HBN],[M_HBN],[SHOHN_NM],
  [SIR_CD],[SIR_NM],
  CONVERT(VARCHAR(8), [EGY_YMD], 112) AS [EGY_YMD],
  [TNP_CD],[SB_NM],[TNP_NM],[DEN_NO],[SKU_CD],
  [SIR_SU],
  SUM(CASE WHEN [SIR_SU] = 0 THEN 0 ELSE [SIR_GK] / [SIR_SU] END) AS [SIR_GT],
  SUM([SIR_GK]) as [SIR_GK],
  SUM([SHP_SU]) as [SHP_SU],
  SUM(CASE WHEN [SHP_SU] = 0 THEN 0 ELSE [SHP_GK] / [SHP_SU] END) AS [SHP_GT],
  SUM([SHP_GK]) as [SHP_GK],
  [CMP_NM]
FROM (
		SELECT
		  [勘定コード],[仕入伝票日付],
		  CI.[年度] AS [Y_DO],
		  CI.[月度] AS [M_DO],
		  CI.[週度] AS [W_DO],
		  [BMN_CD],[J_HBN],[M_HBN],[SHOHN_NM],
		  [SIR_CD],[SIR_NM],[EGY_YMD],[TNP_CD],
		  [SB_NM],[TNP_NM],[DEN_NO],[SKU_CD],
		  [SIR_SU],[商品コード],[SIR_GK],[SHP_SU],
		  [SHP_GK],[CMP_NM]
		FROM (
			SELECT
			  [勘定コード],[仕入伝票日付],
			  [BMN_CD],[J_HBN],[M_HBN],[SHOHN_NM],
			  [SIR_CD],[SIR_NM],[EGY_YMD],[TNP_CD],
			  [SB_NM],TM.[店舗名称] AS [TNP_NM],
			  [DEN_NO],[SKU_CD],[SIR_SU],[商品コード],
			  [SIR_GK],[SHP_SU],[SHP_GK],[CMP_NM]
			FROM (
				SELECT
				  [勘定コード],[仕入伝票日付],
				  [BMN_CD],[J_HBN],[M_HBN],[SHOHN_NM],
				  [SIR_CD],SM.[仕入先名] AS [SIR_NM],
				  [EGY_YMD],[TNP_CD],[SB_NM],[DEN_NO],
				  [SKU_CD],[SIR_SU],[商品コード],[SIR_GK],
				  [SHP_SU],[SHP_GK],[CMP_NM]
				FROM (
					SELECT
					  [勘定コード],[仕入伝票日付],
					  [BMN_CD],[J_HBN],[M_HBN],[SHOHN_NM],
					  [SIR_CD],[EGY_YMD],[TNP_CD],[SB_NM],
					  [DEN_NO],[SKU_CD],[SIR_SU],[商品コード],
					  [SIR_GK],[SHP_SU],[SHP_GK],
					  CASE
						WHEN CHKBUMON.業態コード = 01 THEN 'RO'
						WHEN CHKBUMON.業態コード = 02 THEN 'FR'
						WHEN CHKBUMON.業態コード = 03 THEN 'CH'
						WHEN CHKBUMON.業態コード = 04 THEN 'ND'
						WHEN CHKBUMON.業態コード = 05 THEN 'RG'
						WHEN CHKBUMON.業態コード = 06 THEN 'SP'
						WHEN CHKBUMON.業態コード = 80 THEN 'EC'
						WHEN CHKBUMON.業態コード = 99 THEN 'RO'
						ELSE ''
					  END AS [CMP_NM]
					FROM ( 
						SELECT 
							[勘定コード],[仕入伝票日付],
							SD.部門コード [BMN_CD],
							SD.商品コード [J_HBN],
							SD.メーカー品番 [M_HBN],
							SD.商品テキスト [SHOHN_NM],
							[SIR_CD],[EGY_YMD],[TNP_CD],[SB_NM],
							[DEN_NO],[SKU_CD],[SIR_SU],
							SD.[商品コード] AS [商品コード],
							[SIR_GK],[SHP_SU],[SHP_GK]
						FROM (
							SELECT
							  SDM.勘定コード AS [勘定コード],
							  SDM.仕入伝票日付 [仕入伝票日付],
							  SDM.仕入先コード AS [SIR_CD],
							  SDM.仕入伝票日付 AS [EGY_YMD],
							  SDM.店舗コード AS [TNP_CD],
							  SDM.[店舗コード] AS 店舗コード,
							  CASE
								WHEN 勘定コード = 101 THEN '仕入'
								WHEN 勘定コード IN (961,962) THEN '仕入訂正'
								WHEN 勘定コード = 174 THEN '受託へ振替'
								WHEN 勘定コード = 173 THEN '受託戻し'
								WHEN 勘定コード = 161 THEN '返品'
								WHEN 勘定コード IN (971,972) THEN '返品訂正'
								ELSE ''
							  END AS [SB_NM],
							  SDM.仕入伝票番号 AS [DEN_NO],
							  SDM.SKUコード AS [SKU_CD],
							  CASE
								WHEN 勘定コード IN (101,961,962) THEN 実績数
								WHEN 勘定コード IN (173,174) AND 買取受託='0' THEN 実績数
								ELSE 0
							  END AS SIR_SU,
							  CASE
								WHEN 勘定コード IN (101,961,962) THEN 原価額
								WHEN 勘定コード IN (173,174) AND 買取受託='0' THEN 原価額
								ELSE 0
							  END AS SIR_GT,
							  CASE
								WHEN 勘定コード IN (101,961,962) THEN 原価額
								WHEN 勘定コード IN (173,174) AND 買取受託='0' THEN 原価額
								ELSE 0
							  END AS SIR_GK,
							  CASE
								WHEN 勘定コード IN (161,971,972) THEN 実績数
								ELSE 0
							  END AS SHP_SU,
							  CASE
								WHEN 勘定コード IN (161,971,972) THEN 原価額
								ELSE 0
							  END AS SHP_GT,
							  CASE
								WHEN 勘定コード IN (161,971,972) THEN 原価額
								ELSE 0
							  END AS SHP_GK
							FROM [DBO].[D_SIIRE_DENPYOU_MEISAI] SDM 
							WHERE SDM.仕入伝票日付 BETWEEN (SELECT MIN(STARTDATE) AS STARTDATE FROM (
												SELECT TOP 2 MIN(カレンダー年月日) AS STARTDATE,
													MAX(カレンダー年月日) AS ENDDATE
												FROM MI_SYS_CALENDAR_INFO
												WHERE カレンダー年月日 BETWEEN CONVERT(DATE,DATEADD(MONTH,-2, ? )) AND CONVERT(DATE,DATEADD(DAY,-1, ? ))
												GROUP BY 年度+月度,月度ＳＥＱ
												ORDER BY 年度+月度 DESC
												) AS OLD)
											AND ?  
								AND SDM.勘定コード IN ('101','961','962','174','173','161','971','972')
								AND SDM.買取受託 = '0'
								) AS SDMCHECK
							LEFT JOIN [dbo].[M_SHOUHIN_DATA] SD
							ON SDMCHECK.[SKU_CD] = SD.ＳＫＵコード
							) AS SDMSD
						LEFT JOIN (SELECT DISTINCT 部門コード,業態コード FROM　[dbo].[M_BUMON_MASTER] BM) AS CHKBUMON
						ON CHKBUMON.[部門コード] = SDMSD.[BMN_CD]) AS SDMSDBM
					LEFT JOIN [dbo].[M_SIIRESAKI_MASTER] SM
					ON SM.[仕入先コード] = SDMSDBM.[SIR_CD]
					) AS SDMSDBMSM
				LEFT JOIN [dbo].[M_TENPO_MASTER] TM
				ON TM.[店舗コード] = SDMSDBMSM.[TNP_CD]) AS SDMSDBMSMCI
			LEFT JOIN [dbo].[MI_SYS_CALENDAR_INFO] CI
			ON CI.[カレンダー年月日] = SDMSDBMSMCI.[仕入伝票日付]
			) AS SDMSDBMSMCIGR
		LEFT JOIN [dbo].[M_GENKA_RIREKI] GR
		ON GR.[ＳＫＵコード] = SDMSDBMSMCIGR.[SKU_CD] AND SDMSDBMSMCIGR.仕入伝票日付 between GR.有効開始日 and GR.有効終了日
GROUP BY 
  [Y_DO],[M_DO],[W_DO],[BMN_CD],[J_HBN],
  [M_HBN],[SHOHN_NM],[SIR_CD],[SIR_NM],
  [EGY_YMD],[TNP_CD],[SB_NM],[TNP_NM],
  [DEN_NO],[SKU_CD],[SIR_SU],[CMP_NM]</property>
                <property
