                          name="OpenRowsetVariable"></property>
                        <property
                          dataType="System.String"
                          description="The SQL command to be executed."
                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 仕入先,仕入先名称,日付,店コード,[種別（名称）],店名称,伝票番号,[訂正元購買伝票番号],
SUM(仕入数) AS 仕入数,SUM(仕入金額) AS 仕入金額,
SUM(返品数) AS 返品数,SUM(返品単価) AS 返品単価,SUM(返品金額) AS 返品金額,業態
FROM (
SELECT
	CONVERT(NVARCHAR(100),[仕入先]) AS [仕入先],
	CONVERT(NVARCHAR(100),[仕入先名称]) AS [仕入先名称],
	CAST(CONVERT(VARCHAR(8), [日付], 112) AS NVARCHAR(100)) AS [日付],
	CONVERT(NVARCHAR(100),[店コード]) AS [店コード],
	CONVERT(NVARCHAR(100),[種別（名称）]) AS [種別（名称）],
	CONVERT(NVARCHAR(100),[店名称]) AS [店名称],
	CONVERT(NVARCHAR(100),[伝票番号]) AS [伝票番号],
	CONVERT(NVARCHAR(100),[訂正元購買伝票番号]) AS [訂正元購買伝票番号],
	CONVERT(NVARCHAR(100),[ＪＡＮコード]) AS [ＪＡＮコード],
	[仕入数],
	SUM(CASE
		WHEN 勘定コード = 101 THEN 購買価格
		WHEN 勘定コード = 961 THEN 購買価格
		WHEN 勘定コード = 962 THEN 購買価格
		WHEN 勘定コード = 173 THEN 購買価格
		WHEN 勘定コード = 174 THEN 購買価格
		ELSE 0
	END) AS [仕入単価],
	SUM([仕入金額]) as [仕入金額],
	SUM([返品数]) as [返品数],
	SUM(CASE
		WHEN 勘定コード = 161 THEN 購買価格
		WHEN 勘定コード = 971 THEN 購買価格
		WHEN 勘定コード = 972 THEN 購買価格
		ELSE 0
	END) AS [返品単価],
	SUM([返品金額]) as [返品金額],
	CONVERT(NVARCHAR(100),[業態]) AS [業態]
FROM (
		SELECT
		  [勘定コード],[仕入伝票日付],
		  [部門],[代表品番],[メーカー品番],[商品名],
		  [仕入先],[仕入先名称],[日付],[店コード],
		  [種別（名称）],[店名称],[伝票番号],[訂正元購買伝票番号],[ＪＡＮコード],
		  [仕入数],[商品コード],[仕入金額],[返品数],
		  [返品金額],[業態]
		FROM (
			SELECT
			  [勘定コード],[仕入伝票日付],
			  [部門],[代表品番],[メーカー品番],[商品名],
			  [仕入先],[仕入先名称],[日付],[店コード],
			  [種別（名称）],TM.[店舗名称] AS [店名称],
			  [伝票番号],[訂正元購買伝票番号],[ＪＡＮコード],[仕入数],[商品コード],
			  [仕入金額],[返品数],[返品金額],[業態]
			FROM (
				SELECT
				  [勘定コード],[仕入伝票日付],
				  [部門],[代表品番],[メーカー品番],[商品名],
				  [仕入先],SM.[仕入先名] AS [仕入先名称],
				  [日付],[店コード],[種別（名称）],[伝票番号],[訂正元購買伝票番号],
				  [ＪＡＮコード],[仕入数],[商品コード],[仕入金額],
				  [返品数],[返品金額],[業態]
				FROM (
					SELECT
					  [勘定コード],[仕入伝票日付],
					  [部門],[代表品番],[メーカー品番],[商品名],
					  [仕入先],[日付],[店コード],[種別（名称）],
					  [伝票番号],[訂正元購買伝票番号],[ＪＡＮコード],[仕入数],[商品コード],
					  [仕入金額],[返品数],[返品金額],
					  CASE
						WHEN CHKBUMON.業態コード = 01 THEN 'RO'
						WHEN CHKBUMON.業態コード = 02 THEN 'FR'
						WHEN CHKBUMON.業態コード = 03 THEN 'CH'
						WHEN CHKBUMON.業態コード = 04 THEN 'ND'
						WHEN CHKBUMON.業態コード = 05 THEN 'RG'
						WHEN CHKBUMON.業態コード = 06 THEN 'SP'
						WHEN CHKBUMON.業態コード = 80 THEN 'EC'
						WHEN CHKBUMON.業態コード = 99 THEN 'その他'
						ELSE ''
					  END AS [業態]
					FROM ( 
						SELECT 
							[勘定コード],[仕入伝票日付],
							SD.部門コード [部門],
							SD.商品コード [代表品番],
							SD.メーカー品番 [メーカー品番],
							SD.商品テキスト [商品名],
							[仕入先],[日付],[店コード],[種別（名称）],
							[伝票番号],[訂正元購買伝票番号],[ＪＡＮコード],[仕入数],
							SD.[商品コード] AS [商品コード],
							[仕入金額],[返品数],[返品金額]
						FROM (
							SELECT
							  SDM.勘定コード AS [勘定コード],
							  SDM.仕入伝票日付 [仕入伝票日付],
							  SDM.仕入先コード AS [仕入先],
							  SDM.仕入伝票日付 AS [日付],
							  SDM.店舗コード AS [店コード],
							  SDM.[店舗コード] AS 店舗コード,
							  CASE
								WHEN 勘定コード = 101 THEN '仕入'
								WHEN 勘定コード IN (961,962) THEN '仕入訂正'
								WHEN 勘定コード = 174 THEN '受託へ振替'
								WHEN 勘定コード = 173 THEN '受託戻し'
								WHEN 勘定コード = 161 THEN '返品'
								WHEN 勘定コード IN (971,972) THEN '返品訂正'
								WHEN 勘定コード = 172 THEN '棚卸調整'
								ELSE ''
							  END AS [種別（名称）],
							  SDM.仕入伝票番号 AS [伝票番号],
							  SDM.訂正元購買伝票番号 AS [訂正元購買伝票番号],
							  SDM.SKUコード AS [ＪＡＮコード],
							  CASE
								WHEN 勘定コード IN (101,961,962,172) THEN 実績数
								WHEN 勘定コード IN (173,174) AND 買取受託='0' THEN 実績数
								ELSE 0
							  END AS 仕入数,
							  CASE
								WHEN 勘定コード IN (101,961,962,172) THEN 原価額
								WHEN 勘定コード IN (173,174) AND 買取受託='0' THEN 原価額
								ELSE 0
							  END AS 仕入単価,
							  CASE
								WHEN 勘定コード IN (101,961,962,172) THEN 原価額
								WHEN 勘定コード IN (173,174) AND 買取受託='0' THEN 原価額
								ELSE 0
							  END AS 仕入金額,
							  CASE
								WHEN 勘定コード IN (161,971,972) THEN 実績数
								ELSE 0
							  END AS 返品数,
							  CASE
								WHEN 勘定コード IN (161,971,972) THEN 原価額
								ELSE 0
							  END AS 返品単価,
							  CASE
								WHEN 勘定コード IN (161,971,972) THEN 原価額
								ELSE 0
							  END AS 返品金額
							FROM [DBO].[D_SIIRE_DENPYOU_MEISAI] SDM 
							WHERE SDM.仕入伝票日付 BETWEEN ? AND  ? 
								AND SDM.勘定コード IN ('101','961','962','174','173','161','971','972','172')
								AND SDM.仕入先コード!='199999'
								AND SDM.店舗コード = '7802'
								) AS SDMCHECK
							LEFT JOIN [dbo].[M_SHOUHIN_DATA] SD
							ON SDMCHECK.[ＪＡＮコード] = SD.ＳＫＵコード
							) AS SDMSD
						LEFT JOIN (SELECT DISTINCT 部門コード,業態コード FROM　[dbo].[M_BUMON_MASTER] BM) AS CHKBUMON
						ON CHKBUMON.[部門コード] = SDMSD.[部門]) AS SDMSDBM
					LEFT JOIN [dbo].[M_SIIRESAKI_MASTER] SM
					ON SM.[仕入先コード] = SDMSDBM.[仕入先]
					) AS SDMSDBMSM
				LEFT JOIN [dbo].[M_TENPO_MASTER] TM
				ON TM.[店舗コード] = SDMSDBMSM.[店コード]) AS SDMSDBMSMCI
			LEFT JOIN [dbo].[MI_SYS_CALENDAR_INFO] CI
			ON CI.[カレンダー年月日] = SDMSDBMSMCI.[仕入伝票日付]
			) AS SDMSDBMSMCIGR
		LEFT JOIN [dbo].[M_GENKA_RIREKI] GR
		ON GR.[ＳＫＵコード] = SDMSDBMSMCIGR.[ＪＡＮコード] AND SDMSDBMSMCIGR.仕入伝票日付 between GR.有効開始日 and GR.有効終了日
GROUP BY 
  [仕入先],
  [仕入先名称],
  [日付],
  [店コード],
  [種別（名称）],
  [店名称],
  [伝票番号],
  [訂正元購買伝票番号],
  [ＪＡＮコード],
  [仕入数],
  [業態]
HAVING 仕入先 = ? 
) AS MAIN
GROUP BY 仕入先,仕入先名称,日付,
店コード,[種別（名称）],店名称,
伝票番号,訂正元購買伝票番号,業態</property>
