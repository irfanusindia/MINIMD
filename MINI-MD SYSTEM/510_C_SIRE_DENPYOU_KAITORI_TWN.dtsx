          SQLTask:SqlStatementSource="SELECT&#xA;    DISTNENGETSU,&#xA;    MINDT,&#xA;    MAXDT&#xA;FROM&#xA;    (&#xA;        SELECT&#xA;            CI.年度 + CI.月度 AS DISTNENGETSU,&#xA;            MIN(日付) AS MINDT,&#xA;            MAX(日付) AS MAXDT&#xA;        FROM&#xA;            (&#xA;                SELECT&#xA;                    [勘定コード],&#xA;                    [仕入伝票日付],&#xA;                    [部門],&#xA;                    [代表品番],&#xA;                    [メーカー品番],&#xA;                    [商品名],&#xA;                    [仕入先],&#xA;                    [仕入先名称],&#xA;                    [日付],&#xA;                    [店コード],&#xA;                    [種別（名称）],&#xA;                    TM.[店舗名称] AS [店名称],&#xA;                    [伝票番号],&#xA;                    [ＪＡＮコード],&#xA;                    [仕入数],&#xA;                    [商品コード],&#xA;                    [仕入金額],&#xA;                    [返品数],&#xA;                    [返品金額],&#xA;                    [業態]&#xA;                FROM&#xA;                    (&#xA;                        SELECT&#xA;                            [勘定コード],&#xA;                            [仕入伝票日付],&#xA;                            [部門],&#xA;                            [代表品番],&#xA;                            [メーカー品番],&#xA;                            [商品名],&#xA;                            [仕入先],&#xA;                            SM.[仕入先名] AS [仕入先名称],&#xA;                            [日付],&#xA;                            [店コード],&#xA;                            [種別（名称）],&#xA;                            [伝票番号],&#xA;                            [ＪＡＮコード],&#xA;                            [仕入数],&#xA;                            [商品コード],&#xA;                            [仕入金額],&#xA;                            [返品数],&#xA;                            [返品金額],&#xA;                            [業態]&#xA;                        FROM&#xA;                            (&#xA;                                SELECT&#xA;                                    [勘定コード],&#xA;                                    [仕入伝票日付],&#xA;                                    [部門],&#xA;                                    [代表品番],&#xA;                                    [メーカー品番],&#xA;                                    [商品名],&#xA;                                    [仕入先],&#xA;                                    [日付],&#xA;                                    [店コード],&#xA;                                    [種別（名称）],&#xA;                                    [伝票番号],&#xA;                                    [ＪＡＮコード],&#xA;                                    [仕入数],&#xA;                                    [商品コード],&#xA;                                    [仕入金額],&#xA;                                    [返品数],&#xA;                                    [返品金額],&#xA;                                    CASE&#xA;                                        WHEN CHKBUMON.業態コード = 01 THEN 'RO'&#xA;                                        WHEN CHKBUMON.業態コード = 02 THEN 'FR'&#xA;                                        WHEN CHKBUMON.業態コード = 03 THEN 'CH'&#xA;                                        WHEN CHKBUMON.業態コード = 04 THEN 'SI'&#xA;                                        WHEN CHKBUMON.業態コード = 05 THEN 'RG'&#xA;                                        WHEN CHKBUMON.業態コード = 06 THEN 'SP'&#xA;                                        WHEN CHKBUMON.業態コード = 80 THEN 'EC'&#xA;                                        WHEN CHKBUMON.業態コード = 99 THEN 'その他'&#xA;                                        ELSE ''&#xA;                                    END AS [業態]&#xA;                                FROM&#xA;                                    (&#xA;                                        SELECT&#xA;                                            [勘定コード],&#xA;                                            [仕入伝票日付],&#xA;                                            SD.部門コード [部門],&#xA;                                            SD.商品コード [代表品番],&#xA;                                            SD.メーカー品番 [メーカー品番],&#xA;                                            SD.商品テキスト [商品名],&#xA;                                            [仕入先],&#xA;                                            [日付],&#xA;                                            [店コード],&#xA;                                            [種別（名称）],&#xA;                                            [伝票番号],&#xA;                                            [ＪＡＮコード],&#xA;                                            [仕入数],&#xA;                                            SD.[商品コード] AS [商品コード],&#xA;                                            [仕入金額],&#xA;                                            [返品数],&#xA;                                            [返品金額]&#xA;                                        FROM&#xA;                                            (&#xA;                                                SELECT&#xA;                                                    SDM.勘定コード AS [勘定コード],&#xA;                                                    SDM.仕入伝票日付 [仕入伝票日付],&#xA;                                                    SDM.仕入先コード AS [仕入先],&#xA;                                                    SDM.仕入伝票日付 AS [日付],&#xA;                                                    SDM.店舗コード AS [店コード],&#xA;                                                    SDM.[店舗コード] AS 店舗コード,&#xA;                                                    CASE&#xA;                                                        WHEN 勘定コード = 101 THEN '仕入'&#xA;                                                        WHEN 勘定コード IN(961, 962) THEN '仕入訂正'&#xA;                                                        WHEN 勘定コード = 174 THEN '受託へ振替'&#xA;                                                        WHEN 勘定コード = 173 THEN '受託戻し'&#xA;                                                        WHEN 勘定コード = 161 THEN '返品'&#xA;                                                        WHEN 勘定コード IN(971, 972) THEN '返品訂正'&#xA;                                                        ELSE ''&#xA;                                                    END AS [種別（名称）],&#xA;                                                    SDM.仕入伝票番号 AS [伝票番号],&#xA;                                                    SDM.SKUコード AS [ＪＡＮコード],&#xA;                                                    CASE&#xA;                                                        WHEN 勘定コード IN(101, 961, 962) THEN 実績数&#xA;                                                        WHEN 勘定コード IN(173, 174)&#xA;                                                    AND 買取受託 = '0' THEN 実績数&#xA;                                                        ELSE 0&#xA;                                                    END AS 仕入数,&#xA;                                                    CASE&#xA;                                                        WHEN 勘定コード IN(101, 961, 962) THEN 原価額&#xA;                                                        WHEN 勘定コード IN(173, 174)&#xA;                                                    AND 買取受託 = '0' THEN 原価額&#xA;                                                        ELSE 0&#xA;                                                    END AS 仕入単価,&#xA;                                                    CASE&#xA;                                                        WHEN 勘定コード IN(101, 961, 962) THEN 原価額&#xA;                                                        WHEN 勘定コード IN(173, 174)&#xA;                                                    AND 買取受託 = '0' THEN 原価額&#xA;                                                        ELSE 0&#xA;                                                    END AS 仕入金額,&#xA;                                                    CASE&#xA;                                                        WHEN 勘定コード IN(161, 971, 972) THEN 実績数&#xA;                                                        ELSE 0&#xA;                                                    END AS 返品数,&#xA;                                                    CASE&#xA;                                                        WHEN 勘定コード IN(161, 971, 972) THEN 原価額&#xA;                                                        ELSE 0&#xA;                                                    END AS 返品単価,&#xA;                                                    CASE&#xA;                                                        WHEN 勘定コード IN(161, 971, 972) THEN 原価額&#xA;                                                        ELSE 0&#xA;                                                    END AS 返品金額&#xA;                                                FROM&#xA;                                                    [DBO].[D_SIIRE_DENPYOU_MEISAI] SDM&#xA;                                                WHERE&#xA;                                                    SDM.仕入伝票日付 &#xA;&#x9;&#x9;&#x9;&#x9;NOT BETWEEN (SELECT MIN(月度開始日) AS MINDATE FROM M_DENPYOU_SIME_MASTER WHERE 月度締日 IS NOT NULL)&#xA;&#x9;&#x9;&#x9;&#x9;AND (SELECT MAX(月度終了日) AS MAXDATE FROM M_DENPYOU_SIME_MASTER WHERE 月度締日 IS NOT NULL)&#xA;                                                AND SDM.勘定コード IN('101', '961', '962', '174', '173', '161', '971', '972')&#xA;                                                AND SDM.買取受託 = '0'&#xA;                                                AND SDM.仕入先コード!='199999'&#xA;&#x9;&#x9;&#x9;AND SDM.店舗コード = '7802'&#xA;                                            ) AS SDMCHECK&#xA;                                            LEFT JOIN&#xA;                                                [dbo].[M_SHOUHIN_DATA] SD&#xA;                                            ON  SDMCHECK.[ＪＡＮコード] = SD.ＳＫＵコード&#xA;                                    ) AS SDMSD&#xA;                                    LEFT JOIN&#xA;                                        (&#xA;                                            SELECT&#xA;                                                DISTINCT 部門コード,&#xA;                                                業態コード FROM　[dbo].[M_BUMON_MASTER] BM&#xA;                                        ) AS CHKBUMON&#xA;                                    ON  CHKBUMON.[部門コード] = SDMSD.[部門]&#xA;                            ) AS SDMSDBM&#xA;                            LEFT JOIN&#xA;                                [dbo].[M_SIIRESAKI_MASTER] SM&#xA;                            ON  SM.[仕入先コード] = SDMSDBM.[仕入先]&#xA;                    ) AS SDMSDBMSM&#xA;                    LEFT JOIN&#xA;                        [dbo].[M_TENPO_MASTER] TM&#xA;                    ON  TM.[店舗コード] = SDMSDBMSM.[店コード]&#xA;            ) AS SDMSDBMSMCI&#xA;            LEFT JOIN&#xA;                [dbo].[MI_SYS_CALENDAR_INFO] CI&#xA;            ON  CI.[カレンダー年月日] = SDMSDBMSMCI.[仕入伝票日付]&#xA;            AND 日付 NOT BETWEEN(&#xA;                    SELECT&#xA;                        MIN(月度開始日)&#xA;                    FROM&#xA;                        M_DENPYOU_SIME_MASTER&#xA;                    WHERE&#xA;                        月度締日 IS NOT NULL&#xA;                ) AND (&#xA;                    SELECT&#xA;                        MAX(月度終了日)&#xA;                    FROM&#xA;                        M_DENPYOU_SIME_MASTER&#xA;                    WHERE&#xA;                        月度締日 IS NOT NULL&#xA;                )&#xA;        GROUP BY&#xA;            CI.年度,&#xA;            CI.月度&#xA;    ) AS OLD&#xA;WHERE&#xA;    DISTNENGETSU IS NOT NULL"
