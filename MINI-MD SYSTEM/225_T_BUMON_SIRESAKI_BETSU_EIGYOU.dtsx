                Encoding="UTF8"><![CDATA[#region Namespaces
using System;
using System.Data;
using Microsoft.SqlServer.Dts.Runtime;
using System.Windows.Forms;
using System.Text;
using System.Data;
using System.Data.SqlClient;
using System.IO;
#endregion

namespace ST_8b28edd8aa494aedbc07ba48b45ea0df
{
	[Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
	public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
    {
        #region "Main Method"
        /// <summary>
        /// Description: This method is for select New Data table name selection or creation dynamically.
        ///         thisTableName- shows the table name for latest data month. either created or not created.
        ///         lastTableName- shows the table name for last month, which may call for recovery data insertion.
        /// Created By: Irfan Mansuri
        /// Created On: 2016-11-16
        /// </summary>
		public void Main()
		{
            string strDataDate = Convert.ToDateTime(Dts.Variables["User::today"].Value).ToString("yyyy/MM/dd");
            string strConnectionString = Dts.Variables["User::connectionString"].Value.ToString();
            string prvMonthDt = "";
            StringBuilder strBuildQuery = new StringBuilder();
            SqlConnection myConnection = new SqlConnection(strConnectionString);
            SqlCommand myCommand;
            DataTable dtDataTb;
            StringBuilder strBuild = new StringBuilder();
            try
            {
                dtDataTb = new DataTable();
                _connectionOpen(myConnection);
                strBuildQuery.Clear();
                #region Find Previous Day's NenGetsu
                strBuildQuery.Clear();
                //Get 年度 and 月度 for table name append
                strBuildQuery.Append("SELECT 年度+月度 AS TAB,年度 as YEAR, 月度 as MONTH");
                strBuildQuery.Append(" FROM MI_SYS_CALENDAR_INFO ");
                strBuildQuery.Append(" WHERE カレンダー年月日 = DATEADD(DAY,-1,'" + strDataDate + "')");
                dtDataTb.Reset();
                dtDataTb = _getDataTable(strBuildQuery.ToString(), myConnection);
                Dts.Variables["User::thisMonthRuikeiTable"].Value = "T_DAY_TEN_SKU_UKEBARAI_RUIKEI_" + dtDataTb.Rows[0]["TAB"].ToString();
                Dts.Variables["User::nendo"].Value = dtDataTb.Rows[0]["YEAR"].ToString();
                Dts.Variables["User::getsudo"].Value = dtDataTb.Rows[0]["MONTH"].ToString();
                #endregion

                #region Find Min and Max date of 月度
                dtDataTb = new DataTable();
                _connectionOpen(myConnection);
                strBuildQuery.Clear();
                //Get 年度 and 月度 for table name append
                strBuildQuery.Append("SELECT MIN(カレンダー年月日) as min,MAX(カレンダー年月日) as max ");
                strBuildQuery.Append(" FROM MI_SYS_CALENDAR_INFO ");
                strBuildQuery.Append(" where 年度 = " + Dts.Variables["User::nendo"].Value.ToString()   );
                strBuildQuery.Append(" and 月度 = "   + Dts.Variables["User::getsudo"].Value.ToString() );
                dtDataTb.Reset();
                dtDataTb = _getDataTable(strBuildQuery.ToString(), myConnection);
                Dts.Variables["User::start_date"].Value = dtDataTb.Rows[0]["min"];
                Dts.Variables["User::end_date"].Value = dtDataTb.Rows[0]["max"];
                prvMonthDt = dtDataTb.Rows[0]["min"].ToString();
                #endregion

                #region Find Prv 年度 and 月度
                dtDataTb = new DataTable();
                _connectionOpen(myConnection);
                strBuildQuery.Clear();
                //Get 年度 and 月度 for table name append
                strBuildQuery.Append("SELECT 年度+月度 AS TAB,年度 as YEAR, 月度 as MONTH");
                strBuildQuery.Append(" FROM MI_SYS_CALENDAR_INFO ");
                strBuildQuery.Append(" WHERE カレンダー年月日 = DATEADD(DAY,-1,'" + prvMonthDt + "')");
                dtDataTb.Reset();
                dtDataTb = _getDataTable(strBuildQuery.ToString(), myConnection);
                Dts.Variables["User::prvNendo"].Value = dtDataTb.Rows[0]["YEAR"].ToString();
                Dts.Variables["User::prvGetsu"].Value = dtDataTb.Rows[0]["MONTH"].ToString();
                #endregion

                #region RUIKEI Query 作成
                strBuild.Clear();
                strBuild.Append(" SELECT 年度,月度,CASE WHEN SHN.仕入先コード IS NULL OR  SHN.仕入先コード = '' THEN '#' ELSE SHN.仕入先コード END AS 仕入先コード, CASE WHEN( ");
                strBuild.Append(" UKEBARAI.部門コード IS NULL OR  UKEBARAI.部門コード = '' ) THEN '#' ELSE UKEBARAI.部門コード END AS 部門コード, 仕入点数, 仕入原価, 仕入売価, ");
                strBuild.Append(" 仕入返品点数,仕入返品原価,仕入返品売価,仕入値引合計金額,CASE WHEN 買取受託 = '0' THEN 振入点数 ELSE 0 END AS 振入点数, CASE WHEN 買取受託 = '0' THEN 振入原価 ");
                strBuild.Append(" ELSE 0 END AS 振入原価, CASE WHEN 買取受託 = '0' THEN 振入売価 ELSE 0 END AS 振入売価, CASE WHEN 買取受託 = '0' THEN 振出点数 ELSE 0 END AS 振出点数, ");
                strBuild.Append(" CASE WHEN 買取受託 = '0' THEN 振出原価 ELSE 0 END AS 振出原価, CASE WHEN 買取受託 = '0' THEN 振出売価 ELSE 0 END AS 振出売価, 売上点数, 売上金額, ");
                strBuild.Append(" 売上返品点数, 売上返品金額, 売上値引金額, 調整数量, 調整原価, 調整売価, CASE WHEN 買取受託 = '0' THEN 売変金額 ELSE 0 END AS 売変, ");
                strBuild.Append(" CASE WHEN 買取受託 = '0' THEN 棚卸ロス数 ELSE 0 END AS ロス数量, CASE WHEN 買取受託 = '0' THEN 棚卸ロス原価 ELSE 0 END AS ロス原価, CASE ");
                strBuild.Append(" WHEN 買取受託 = '0' THEN 棚卸ロス売価 ELSE 0 END AS ロス売価, CASE WHEN 買取受託 = '2' THEN 棚卸ロス数 ELSE 0 END AS [ロス数量(受託) ],CASE ");
                strBuild.Append(" WHEN 買取受託 = '2' THEN 棚卸ロス原価 ELSE 0 END AS [ロス原価(受託) ], CASE WHEN 買取受託 = '2' THEN 棚卸ロス売価 ELSE 0 END AS [ロス売価(受託) ], ");
                strBuild.Append(" リベート, ロイヤリティ, CASE WHEN 買取受託 = '0' THEN [在庫数量] ELSE 0 END AS [在庫数量], CASE WHEN 買取受託 = '0' THEN [在庫原価(移)] ELSE 0 ");
                strBuild.Append(" END AS [在庫原価(移) ], CASE WHEN 買取受託 = '0' THEN [在庫売価(移)] ELSE 0 END AS [在庫売価(移) ], CASE WHEN 買取受託 = '0' THEN [在庫原価] ELSE 0 ");
                strBuild.Append(" END AS [在庫原価], CASE WHEN 買取受託 = '0' THEN [在庫売価] ELSE 0 END AS [在庫売価] FROM " + Dts.Variables["User::thisMonthRuikeiTable"].Value + " AS UKEBARAI ");
                strBuild.Append(" LEFT JOIN M_SHOUHIN_DATA AS SHN ON  SHN.ＳＫＵコード = UKEBARAI.SKUコード AND SHN.部門コード = UKEBARAI.部門コード WHERE ");
                strBuild.Append(" UKEBARAI.SKUコード NOT IN('2082100000010', '2088100000029', '2081100000013') AND UKEBARAI.部門コード!='' AND UKEBARAI.部門コード IS NOT NULL ");
                strBuild.Append(" AND SHN.仕入先コード !='' ANd SHN.仕入先コード IS NOT NULL AND 年度 = '" + Dts.Variables["User::nendo"].Value + "' AND 月度 = '" + Dts.Variables["User::getsudo"].Value + "' AND SHN.部門コード NOT IN ('081','082','083','084','085','086','087','088','089');");
                Dts.Variables["User::ruikeiSyukeiSql"].Value = strBuild.ToString();
                #endregion
            }
            #region CATCH/FINALLY
            catch (Exception ex)
            {
                //システムエラーの場合。
                strBuildQuery.Clear();
                strBuildQuery.Append("INSERT INTO ");
                strBuildQuery.Append("KANSHI_PACKAGE_ERROR([パッケージコード],[データ日付],[エラー日時],[エラー内容],[エラーフラグ]) ");
                strBuildQuery.Append("VALUES('BAIKA999','" + strDataDate + "','" + DateTime.Now + "','システムエラー: " + ex.Message.Replace("'", "-") + "',3);");
                _connectionOpen(myConnection);
                myCommand = new SqlCommand(strBuildQuery.ToString(), myConnection);
                myCommand.ExecuteNonQuery();
                _connectionClose(myConnection);
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
            finally
            {
                _connectionClose(myConnection);
                strBuildQuery = null;
                strConnectionString = null;
                dtDataTb = null;
                myCommand = null;
                myConnection = null;
                strDataDate = null;
            }
            #endregion
            Dts.TaskResult = (int)ScriptResults.Success;
		}
        #endregion

        #region "Select Data From DB"
        /// <summary>
        /// 作成人: イルファン マンスリー
        /// 作成日: 2016-11-16
        /// 説明: Select Data in Data table format from Db.
        /// </summary>
        public DataTable _getDataTable(string strQuery, SqlConnection myConnection)
        {
            DataTable objDT = new DataTable();
            StringBuilder strBrQuery = new StringBuilder();
            SqlCommand myCommand;
            string strCurDate = Convert.ToDateTime(Dts.Variables["User::today"].Value).ToString("yyyy/MM/dd");
            try
            {
                //ＤＢからリターンデータテーブル。
                _connectionOpen(myConnection);
                myCommand = new SqlCommand(strQuery, myConnection);
                myCommand.CommandType = CommandType.Text;
                SqlDataAdapter sqlSelectData = new SqlDataAdapter();
                sqlSelectData.SelectCommand = myCommand;
                sqlSelectData.Fill(objDT);
            }
            #region "CATCH/FINALLY"
            catch (Exception ex)
            {
                //システムエラーの場合。
                strBrQuery.Clear();
                strBrQuery.Append("INSERT INTO ");
                strBrQuery.Append("KANSHI_PACKAGE_ERROR([パッケージコード],[データ日付],[エラー日時],[エラー内容],[エラーフラグ]) ");
                strBrQuery.Append("VALUES('UK999','" + strCurDate + "','" + DateTime.Now + "','システムエラー: " + ex.Message.Replace("'", "-") + "',3);");
                _connectionOpen(myConnection);
                myCommand = new SqlCommand(strBrQuery.ToString(), myConnection);
                myCommand.ExecuteNonQuery();
                _connectionClose(myConnection);
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
            finally
            {
                myConnection = null;
                strBrQuery = null;
                myCommand = null;
            }
            #endregion
            return objDT;
        }
        #endregion

        #region "SQLConnection Open"
        //Existing Connection Open
        private void _connectionOpen(SqlConnection myConnection)
        {
            if (myConnection.State != ConnectionState.Open)
            {
                myConnection.Open();
            }
        }
        #endregion

        #region "SQLConnection Close"
        //Existing Connection Close
        private void _connectionClose(SqlConnection myConnection)
        {
            if (myConnection.State != ConnectionState.Closed)
            {
                myConnection.Close();
            }
        }
        #endregion

        #region ScriptResults declaration
