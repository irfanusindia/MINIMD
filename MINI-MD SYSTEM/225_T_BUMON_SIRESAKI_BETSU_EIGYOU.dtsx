                <component
                  refId="Package\Foreach Loop Container\部門仕入先別営業報告書\エラーJAN値引"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  description="OLE DB Source"
                  name="エラーJAN値引"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	MST.部門コード,
	ERRO_JAN_FINAL.仕入先コード,
	(SUM(売上値引金額)) as 売上値引金額
FROM (
	SELECT
		md.伝票日付,
		md.店舗コード,
		md.仕入先コード,
		md.品目コード,
		SUM(md.売上数) AS 売上数量,
		SUM(md.売上金額) AS 売上金額,
		SUM(md.返品数) AS 返品数量,
		SUM(md.返品金額) AS 返品金額,
		SUM(ABS(md.売上割引) - ABS(md.返品割引)) AS 売上値引金額,
		'TC08' as 税コード
	FROM
		(
        SELECT
			CASE
                WHEN uriage.仕入先コード IS NULL THEN henpin.仕入先コード
                ELSE uriage.仕入先コード
            END AS 仕入先コード,
			CASE
                WHEN uriage.店舗コード IS NULL THEN henpin.店舗コード
                ELSE uriage.店舗コード
            END AS 店舗コード,
            CASE
                WHEN uriage.SKUコード IS NULL THEN henpin.SKUコード
                ELSE uriage.SKUコード
            END AS 品目コード,
            CASE
                WHEN uriage.売上伝票日付 IS NULL THEN henpin.売上伝票日付
                ELSE uriage.売上伝票日付
            END AS 伝票日付,
            CASE
                WHEN uriage.売上数 IS NULL THEN 0
                ELSE uriage.売上数
            END AS 売上数,
            CASE
                WHEN uriage.売上売価 IS NULL THEN 0
                ELSE uriage.売上売価
            END AS 売上金額,
            CASE
                WHEN uriage.売上割引 IS NULL THEN 0
                ELSE uriage.売上割引
            END AS 売上割引,
            CASE
                WHEN henpin.売上数 IS NULL THEN 0
                ELSE henpin.売上数
            END AS 返品数,
            CASE
                WHEN henpin.売上売価 IS NULL THEN 0
                ELSE henpin.売上売価
            END AS 返品金額,
            CASE
                WHEN henpin.売上割引 IS NULL THEN 0
                ELSE henpin.売上割引
            END AS 返品割引
        FROM
            (
                SELECT
						仕入先コード,
						店舗コード,
                        SKUコード,
                        売上伝票日付,
                        売上売価,
                        売上数,
                        売上割引,
                        勘定コード,
                        税コード
                FROM
                    D_URIAGE_DENPYOU_MEISAI AS tum1
                WHERE
                    (
                        売上伝票日付 BETWEEN   ?  AND  ?  
                    )
                AND (
                        勘定コード  = '801'
                    )
				AND SKUコード IN (
								SELECT
									SKUコード
								FROM
									M_SHOUHIN_DATA AS M_ERROR_JAN_1
								WHERE SKUコード LIKE '3%'
                            )
            ) AS uriage
            FULL OUTER JOIN
                (
                    SELECT
                        仕入先コード,
						店舗コード,
                        SKUコード,
                        売上伝票日付,
                        売上売価,
                        売上数,
                        売上割引,
                        勘定コード,
                        税コード
                    FROM
                        D_URIAGE_DENPYOU_MEISAI AS tum1
                    WHERE
                        (
                            売上伝票日付 BETWEEN   ?  AND  ? 
                        )
                    AND (
                            勘定コード = '802'
                        )
					AND SKUコード IN (
								SELECT
									SKUコード
								FROM
									M_SHOUHIN_DATA AS M_ERROR_JAN_1
								WHERE SKUコード LIKE '3%'
                            )
                ) AS henpin
            ON  uriage.仕入先コード=henpin.仕入先コード
			AND uriage.店舗コード = henpin.店舗コード
            AND uriage.売上伝票日付 = henpin.売上伝票日付
            AND uriage.SKUコード = henpin.SKUコード
            AND uriage.税コード = henpin.税コード
		) AS md
	GROUP BY
		md.伝票日付,
		md.店舗コード,
		md.仕入先コード,
		md.品目コード
UNION ALL
		SELECT
			伝票日付,
			店舗コード,
			仕入先コード,
			SKUコード,
			SUM(売上数量) as 売上数量,
			SUM(売上売価) as 売上金額,
			SUM(返品数量) as 返品数量,
			SUM(返品金額) as 返品金額,
			SUM(売上販売値引 - 返品販売値引) as 売上値引金額,
			'TC08' as 税コード
		FROM (
			SELECT
				売上伝票日付 as 伝票日付,
				店舗コード,
				MST.仕入先コード,
				ERR.SKUコード,
				売上数 as 売上数量,
				売上売価,
				売上値引 as 売上販売値引, 
				0 as 返品数量,
				0 as 返品金額,
				0 as 返品販売値引
			FROM
				T_DAY_TEN_ERROR_JAN_NEBIKI AS ERR
			LEFT JOIN M_SHOUHIN_DATA AS MST
				ON MST.ＳＫＵコード=ERR.SKUコード
			WHERE 売上伝票日付 BETWEEN   ?  AND  ?  AND 勘定コード = '801'
			UNION ALL
			SELECT
				売上伝票日付 as 伝票日付,
				店舗コード,
				MST.仕入先コード,
				ERR.SKUコード,
				0 as 売上数量,
				0 as 売上金額,
				0 as 売上販売値引,
				売上数 as 返品数量,
				売上売価 as 返品金額,
				売上値引 as 返品販売値引
			FROM
				T_DAY_TEN_ERROR_JAN_NEBIKI AS ERR
			LEFT JOIN M_SHOUHIN_DATA AS MST
				ON MST.ＳＫＵコード=ERR.SKUコード
			WHERE 売上伝票日付 BETWEEN   ?  AND  ?    AND 勘定コード = '802'
		) ERRORJAN
	GROUP BY
		伝票日付,
		店舗コード,
		仕入先コード,
		SKUコード 
	) ERRO_JAN_FINAL
LEFT JOIN M_SHOUHIN_DATA MST
ON MST.ＳＫＵコード = ERRO_JAN_FINAL.品目コード
GROUP BY
	MST.部門コード,ERRO_JAN_FINAL.仕入先コード
ORDER BY
    MST.部門コード,ERRO_JAN_FINAL.仕入先コード</property>
