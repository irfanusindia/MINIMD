                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="コマンドがタイムアウトするまでの秒数。値 0 はタイムアウトしないことを表します。"
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="行セットを開くために使用するデータベース オブジェクトの名前を指定します。"
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="行セットを開くために使用するデータベース オブジェクトの名前を含む変数を指定します。"
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="実行する SQL コマンド。"
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
    部門コード,
   仕入先コード,
    ABS((SUM(リベット金額)) - SUM(ロイヤリティ)) as 仕入値引合計金額,
    SUM(ロイヤリティ) as ロイヤリティ
FROM
    (
        select
            部門コード,
            仕入先コード,
            勘定コード,
            SUM(国内通貨額) as リベット金額,
            0 as ロイヤリティ
        from
            D_KAIKEI_DENPYOU_MEISAI
        where
            会計伝票日付 BETWEEN ? AND ?  
        AND 勘定コード = '701'
        group by
            部門コード,
            仕入先コード,
            勘定コード
        UNION ALL
        select
            部門コード,
            仕入先コード,
            勘定コード,
            0 as リベット金額,
            SUM(国内通貨額) as ロイヤリティ
        from
