      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="ruikeiSyukeiSql">
      <DTS:VariableValue
        DTS:DataType="8">SELECT     年度,     月度,  CASE  		WHEN SHN.仕入先コード IS NULL OR SHN.仕入先コード = '' THEN '#' 		ELSE SHN.仕入先コード 	END AS 仕入先コード,   CASE         WHEN(             UKEBARAI.部門コード IS NULL         OR  UKEBARAI.部門コード = ''         ) THEN '#'         ELSE UKEBARAI.部門コード     END AS 部門コード, 	    仕入点数,     仕入原価,     仕入売価,     仕入返品点数,     仕入返品原価,     仕入返品売価,     仕入値引合計金額,     CASE         WHEN 買取受託 = '0' THEN 振入点数         ELSE 0     END AS 振入点数,     CASE         WHEN 買取受託 = '0' THEN 振入原価         ELSE 0     END AS 振入原価,     CASE         WHEN 買取受託 = '0' THEN 振入売価         ELSE 0     END AS 振入売価,     CASE         WHEN 買取受託 = '0' THEN 振出点数         ELSE 0     END AS 振出点数,     CASE         WHEN 買取受託 = '0' THEN 振出原価         ELSE 0     END AS 振出原価,     CASE         WHEN 買取受託 = '0' THEN 振出売価         ELSE 0     END AS 振出売価,     売上点数,     売上金額,     売上返品点数,     売上返品金額,     売上値引金額,     調整数量,     調整原価,     調整売価,     CASE         WHEN 買取受託 = '0' THEN 売変金額         ELSE 0     END AS 売変,     CASE         WHEN 買取受託 = '0' THEN 棚卸ロス数         ELSE 0     END AS ロス数量,     CASE         WHEN 買取受託 = '0' THEN 棚卸ロス原価         ELSE 0     END AS ロス原価,     CASE         WHEN 買取受託 = '0' THEN 棚卸ロス売価         ELSE 0     END AS ロス売価,     CASE         WHEN 買取受託 = '2' THEN 棚卸ロス数         ELSE 0     END AS [ロス数量(受託) ],     CASE         WHEN 買取受託 = '2' THEN 棚卸ロス原価         ELSE 0     END AS [ロス原価(受託) ],     CASE         WHEN 買取受託 = '2' THEN 棚卸ロス売価         ELSE 0     END AS [ロス売価(受託) ],     リベート,     ロイヤリティ,     CASE         WHEN 買取受託 = '0' THEN [在庫数量]         ELSE 0     END AS [在庫数量],     CASE         WHEN 買取受託 = '0' THEN [在庫原価(移) ]         ELSE 0     END AS [在庫原価(移) ],     CASE         WHEN 買取受託 = '0' THEN [在庫売価(移) ]         ELSE 0     END AS [在庫売価(移) ],     CASE         WHEN 買取受託 = '0' THEN [在庫原価]         ELSE 0     END AS [在庫原価],     CASE         WHEN 買取受託 = '0' THEN [在庫売価]         ELSE 0     END AS [在庫売価] FROM T_DAY_TEN_SKU_UKEBARAI_RUIKEI AS UKEBARAI LEFT JOIN M_SHOUHIN_DATA AS SHN ON SHN.ＳＫＵコード=UKEBARAI.SKUコード AND SHN.部門コード=UKEBARAI.部門コード WHERE  UKEBARAI.SKUコード NOT IN('2082100000010', '2088100000029', '2081100000013') AND 年度='2017' AND 月度='11' AND SHN.部門コード NOT IN ('081','082','083','084','085','086','087','088','089')</DTS:VariableValue>
    </DTS:Variable>
