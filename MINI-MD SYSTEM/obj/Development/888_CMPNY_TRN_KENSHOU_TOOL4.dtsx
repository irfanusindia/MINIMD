                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT ISNULL(REGI.売上計上日,CMP.伝票日付) AS 売上日付,
	ISNULL(REGI.商品売上額,0) AS CMP商品売上額,
	ISNULL(REGI.商品売上額,0) - ISNULL(CMP.金額,0) AS DIFF商品売上額,
	ISNULL(CMP.金額,0) AS 金額
	FROM (
	SELECT 売上計上日 ,SUM(商品売上額) AS 商品売上額
	FROM T_DAY_TEN_REGI_SEISAN
	WHERE 売上計上日 BETWEEN ? AND  ? 
	GROUP BY 売上計上日 
	HAVING SUM(商品売上額) != 0
	) AS REGI
	FULL JOIN
	(
		SELECT 伝票日付 AS 伝票日付,
		SUM(CASE WHEN 貸借区分= '0' THEN ((-1)*金額) ELSE 金額 END) AS 金額
		FROM TOOL4_CMPNY_URIAGE_TRN
		WHERE 伝票日付 BETWEEN  ? AND  ? 
		GROUP BY 伝票日付
	) AS CMP
ON CMP.伝票日付 = REGI.売上計上日
ORDER BY CMP.伝票日付,REGI.売上計上日</property>
