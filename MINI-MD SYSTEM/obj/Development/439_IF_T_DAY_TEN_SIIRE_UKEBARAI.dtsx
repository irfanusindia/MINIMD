                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
    CAL.年度,
    CAL.月度,
    CONVERT(VARCHAR(8),SIIRE.仕入伝票日付,112) AS 運用日,
    SIIRE.店舗コード,
    MST.部門コード,
    SIIRE.SKUコード,
    SIIRE.買取受託,
    SUM(CASE WHEN 勘定コード = '101' OR 勘定コード = '961' OR 勘定コード = '962' OR 勘定コード = '171' OR 勘定コード = '172' OR
    (買取受託 = '0' AND (勘定コード = '173' OR 勘定コード = '174')) THEN 実績数 ELSE 0 END) AS 仕入点数,
    SUM(CASE WHEN 勘定コード = '101' OR 勘定コード = '961' OR 勘定コード = '962' OR 勘定コード = '171' OR 勘定コード = '172' OR
    (買取受託 = '0' AND (勘定コード = '173' OR 勘定コード = '174')) THEN 原価額 ELSE 0 END) AS 仕入原価,
    SUM(CASE WHEN 勘定コード = '101' OR 勘定コード = '961' OR 勘定コード = '962' OR 勘定コード = '171' OR 勘定コード = '172' OR
    (買取受託 = '0' AND (勘定コード = '173' OR 勘定コード = '174')) THEN 売価額 ELSE 0 END) AS 仕入売価,
    SUM(CASE WHEN 勘定コード = '161' OR 勘定コード = '971' OR 勘定コード = '972' THEN 実績数 ELSE 0 END) AS 仕入返品点数,
    SUM(CASE WHEN 勘定コード = '161' OR 勘定コード = '971' OR 勘定コード = '972' THEN 原価額 ELSE 0 END) AS 仕入返品原価,
    SUM(CASE WHEN 勘定コード = '161' OR 勘定コード = '971' OR 勘定コード = '972' THEN 売価額 ELSE 0 END) AS 仕入返品売価
FROM DBO.D_SIIRE_DENPYOU_MEISAI AS SIIRE LEFT OUTER JOIN
