              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
    '1000' AS CORP_CODE,
    'INV02' AS IF_SYSTEM_FUNC_ID,
    LEFT(CONVERT(VARCHAR, '20150920'  ,112),6) + '02' + RIGHT('00000' + EGY.店舗コード, '5') AS IF_SYSTEM_NO,
    '' AS CORRECT_DIV,
    '' AS CORRECT_IF_SYSTEM_NO,
    LEFT(CONVERT(VARCHAR,DATEADD(DAY,1, '20150920' ),112),6) + '21' AS BOOK_DATE,
    'IF9999' AS BOOK_USER_ID,
    'IF9999' AS H_SECTION_CODE,
    '0' AS TERM_DIV,
    '10' AS SLIP_TYPE,
    月度 + '月度棚卸計上' AS HD_MEMO_TXT,
    ROW_NUMBER() OVER(PARTITION BY EGY.店舗コード ORDER BY EGY.店舗コード) AS ROW_NO,
    '1' AS D_AND_C_DIV,
    RIGHT('00000' + EGY.店舗コード, '5') AS SECTION_CODE,
    '1210900' AS ITEM_CODE,
    (月末在庫原価) AS AMOUNT,
    MST.店舗名称 + 月度 + '月度棚卸戻し' AS DTL_MEMO_TEXT
FROM
    T_TENPO_EIGYOU_HOKOKUSHOU EGY
LEFT JOIN
    M_TENPO_MASTER MST
ON  EGY.店舗コード = MST.店舗コード
WHERE
    年度 = ? AND　月度 = ?
    AND 月末在庫原価 !=0</property>
