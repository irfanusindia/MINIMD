                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
    '1000' AS CORP_CODE,
    'INV01' AS IF_SYSTEM_FUNC_ID,
    LEFT(CONVERT(varchar, '20150920'  ,112),6) + '01' + RIGHT('00000' + EGY.店舗コード, '5') AS IF_SYSTEM_NO,
    '' AS CORRECT_DIV,
    '' AS CORRECT_IF_SYSTEM_NO,
    LEFT(CONVERT(varchar, '20150920'  ,112),6) + '20' AS BOOK_DATE,
    'IF9999' AS BOOK_USER_ID,
    'IF9999' AS H_SECTION_CODE,
    '0' AS TERM_DIV,
    '10' AS SLIP_TYPE,
    月度 + '月度棚卸計上' AS HD_MEMO_TXT,
    CAST('01' AS BIGINT) AS ROW_NO,
    '0' AS D_AND_C_DIV,
    RIGHT('00000' + EGY.店舗コード, '5') AS SECTION_CODE,
    '8295100' AS ITEM_CODE,
    ロス売価 AS AMOUNT,
    MST.店舗名称 + 月度 + '月度棚卸戻し' AS DTL_MEMO_TEXT,
    EGY.店舗コード
FROM
    T_TENPO_EIGYOU_HOKOKUSHOU EGY
LEFT JOIN
    M_TENPO_MASTER MST
ON  EGY.店舗コード = MST.店舗コード
WHERE 年度 = ? AND　月度 = ?</property>
