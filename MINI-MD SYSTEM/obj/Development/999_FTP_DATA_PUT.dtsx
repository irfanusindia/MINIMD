using System;
using System.Data;
using System.Data.SqlClient;
using System.IO;
using System.Text;
using Microsoft.SqlServer.Dts.Runtime;
using System.Windows.Forms;
#endregion

namespace ST_78cb68fb87b2493686f2f983579f215d
{
    /// <summary>
    /// ScriptMain is the entry point class of the script.  Do not change the name, attributes,
    /// or parent of this class.
    /// </summary>
	[Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
	public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
	{
		public void Main()
		{
            StringBuilder strBuildQry = new StringBuilder();
            string strConnectionString = Dts.Variables["connectionString"].Value.ToString();
            SqlConnection myconnection = new SqlConnection(strConnectionString);
            DataTable dtTemp = null;
            string strDbData = string.Empty;
            string strPath = Dts.Variables["User::savePath"].Value.ToString();
            StreamWriter stream = null;
            try
            {
                dtTemp = new DataTable();
                if(myconnection.State == ConnectionState.Closed)
                {
                    myconnection.Open();
                }
                strBuildQry.Append(" SELECT  [コントロールファイル記述] FROM [mini_md_db].[dbo].[M_COMPANY_CTRL_MST] WHERE ファイル名 = '" + Dts.Variables["fileName"].Value.ToString() + "'; ");
                dtTemp = _getDataTable(strBuildQry.ToString(), myconnection);
                Dts.Variables["User::dataFilePath"].Value = Dts.Variables["User::dataPath"].Value.ToString() + Dts.Variables["fileName"].Value.ToString() + "_" + DateTime.Now.ToString("yyyyMMdd") + ".TSV";
                if (dtTemp.Rows.Count > 0)
                {
                    strDbData = dtTemp.Rows[0][0].ToString();
                    Dts.Variables["User::newFile"].Value = strDbData + " " + Dts.Variables["User::fileName"].Value.ToString() + "_" + DateTime.Now.ToString("yyyyMMdd") + ".TSV";
                    strPath = strPath + Dts.Variables["User::fileName"].Value.ToString() + "_CTRL.TSV";
                    Dts.Variables["User::cntrFilePath"].Value = strPath;
                    if (!File.Exists(strPath))
                    {
                        File.Create(strPath).Close();
                    }
                    stream = new StreamWriter(strPath);
                    stream.WriteLine(Dts.Variables["User::newFile"].Value.ToString());
                }
                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception)
            {
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
            finally 
            {
                dtTemp = null;
                strPath = null;
                if(myconnection.State == ConnectionState.Open)
                {
                    myconnection.Close();
                }
                strDbData = null;
                strConnectionString = null;
                myconnection = null;
                strBuildQry = null;
                stream.Flush();
                stream.Close();
                if (stream != null)
                {
                    stream = null;
                }
            }
		}


        #region "Get DataTable"
        //Created On: 08-08-2016
        //Description: Aim to Return DataTable from the Query.
        public DataTable _getDataTable(string strQuery, SqlConnection myConnection)
        {
            StringBuilder strERRQuery = new StringBuilder();
            DataTable objDT = new DataTable();
            SqlCommand myCommand;
            SqlDataAdapter sqlSelectData;
            try
            {
                if (myConnection.State == ConnectionState.Closed)
                {
                    myConnection.Open();
                }
                myCommand = new SqlCommand(strQuery, myConnection);
                myCommand.CommandType = CommandType.Text;
                sqlSelectData = new SqlDataAdapter();
                sqlSelectData.SelectCommand = myCommand;
                sqlSelectData.Fill(objDT);
            }
            #region CATCH/FINALLY
            catch (Exception ex)
            {
                strERRQuery.Clear();
                strERRQuery.Append("INSERT INTO KANSHI_PACKAGE_ERROR ");
                strERRQuery.Append(" (パッケージコード,データ日付,エラー日時,エラー内容,エラーフラグ) ");
                strERRQuery.Append(" VALUES('GMCHCK','" + Dts.Variables["User::currentDate"].Value.ToString() + "','" + Dts.Variables["User::sysDate"].Value.ToString() + "','" + ex.Message.ToString().Replace("'", "-") + "','3');");
                _updateInsert(strERRQuery.ToString(), myConnection);
            }
            finally
            {
                strQuery = null;
                myCommand = null;
                sqlSelectData = null;
                strERRQuery = null;
            }
            #endregion
            return objDT;
        }
        #endregion

        #region "Update/Insert Query in Db"
        protected void _updateInsert(string strQuery, SqlConnection myConnection)
        {
            StringBuilder strERRQuery = new StringBuilder();
            SqlCommand myCommand;
            try
            {
                myCommand = new SqlCommand(strQuery, myConnection);
                if (myConnection.State == ConnectionState.Closed)
                {
                    myConnection.Open();
                }
                myCommand.ExecuteNonQuery();
            }
            #region CATCH/FINALLY
            catch (Exception ex)
            {
                throw ex;
            }
            finally
            {
                strERRQuery = null;
                myConnection = null;
                myCommand = null;
            }
            #endregion
        }
        #endregion

        #region ScriptResults declaration
        /// <summary>
        /// This enum provides a convenient shorthand within the scope of this class for setting the
        /// result of the script.
        /// 
        /// This code was generated automatically.
        /// </summary>
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

	}
}]]></ProjectItem>
