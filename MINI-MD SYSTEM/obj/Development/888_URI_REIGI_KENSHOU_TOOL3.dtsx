                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	ISNULL(URI.売上計上日,REGI.売上計上日) AS 売上計上日,
	ISNULL(URI.売上数量,0) AS URI純売上数,
	ISNULL(URI.純売上金額,0) AS URI売上金額,
	ISNULL(URI.売上数量,0) - ISNULL(REGI.純売上数,0) AS DIFF純売上数,
	ISNULL(URI.純売上金額,0) - ISNULL(REGI.純売上額,0) AS DIFF純売上金額,
	ISNULL(REGI.純売上数,0) AS REGI純売上数,
	ISNULL(REGI.純売上額,0) AS REGI純売上額
FROM (
		SELECT
			ISNULL(D_URIAGE.運用日付,ERROR_JAN.伝票日付) AS 売上計上日,
			ISNULL(D_URIAGE.売上点数,0) AS 売上数量,
			ISNULL(D_URIAGE.純売上金額,0) - ISNULL(ERROR_JAN.売上値引金額,0) AS 純売上金額
		FROM (
		SELECT
			URIAGE_MEISAI.運用日付,
			SUM(CAST(URIAGE_MEISAI.売上数量 AS INT)) - SUM(CAST(URIAGE_MEISAI.返品数量 AS INT)) AS 売上点数,
			SUM(ISNULL(URIAGE_MEISAI.売上金額, 0)) - (SUM(ISNULL(URIAGE_MEISAI.返品金額, 0))) AS 純売上金額
		FROM
			(
				SELECT
					伝票日付 AS 運用日付,
					店舗コード,
					品目コード AS SKUコード,
					SUM(売上数量) AS 売上数量,
					SUM(売上金額) AS 売上金額,
					SUM(返品数量) AS 返品数量,
					SUM(返品金額) AS 返品金額,
					SUM(売上値引金額) AS 売上値引金額
				FROM
					(
						SELECT
							MD.伝票日付,
							MD.店舗コード,
							MD.品目コード,
							SUM(MD.売上数量) AS 売上数量,
							SUM(MD.売上金額 + MD.売上割引) AS 売上金額,
							SUM(MD.返品数量) AS 返品数量,
							SUM(MD.返品金額 + MD.返品割引) AS 返品金額,
							0 AS 売上値引金額,
							MD.税コード
						FROM
							(
								SELECT
									店舗コード,
									SKUコード AS 品目コード,
									売上伝票日付 AS 伝票日付,
									売上数 AS 売上数量,
									売上売価 AS 売上金額,
									0 AS 売上割引,
									0 AS 返品数量,
									0 AS 返品金額,
									0 AS 返品割引,
									税コード
								FROM
									DBO.D_URIAGE_DENPYOU_MEISAI AS TUM1
								WHERE
									(
										売上伝票日付 BETWEEN ? AND ? 
									)
								AND (
										勘定コード = '801'
									)
								AND (SKUコード IN(
											SELECT
														SKUコード
													FROM
														M_SHOUHIN_DATA AS M_ERROR_JAN_1
													WHERE SKUコード LIKE '3%'
										))
								AND (NOT EXISTS(
											SELECT
												売上伝票日付,
												発生日,
												勘定コード,
												店舗コード,
												SKUコード,
												買取受託,
												売上数,
												売上売価,
												売上値引,
												トランゼクションID,
												税コード,
												登録日時
											FROM
												DBO.T_DAY_TEN_ERROR_JAN_NEBIKI AS ERR
											WHERE
												(
													売上伝票日付 = TUM1.売上伝票日付
												)
											AND (
													店舗コード = TUM1.店舗コード
												)
											AND (
													SKUコード = TUM1.SKUコード
												)
											AND (
													勘定コード = '801'
												)
										))
                                                                                                                                                     OR (店舗コード='8012' ANd 勘定コード='801')
								UNION ALL
								SELECT
									店舗コード,
									SKUコード AS 品目コード,
									売上伝票日付 AS 伝票日付,
									0 AS 売上数量,
									0 AS 売上金額,
									0 AS 売上割引,
									売上数 AS 返品数量,
									売上売価 AS 返品金額,
									0 AS 返品割引,
									税コード
								FROM
									DBO.D_URIAGE_DENPYOU_MEISAI AS TUM1
								WHERE
									(
										売上伝票日付 BETWEEN  ? AND ? 
									)
								AND (
										勘定コード = '802'
									)
								AND (SKUコード IN(
											SELECT
														SKUコード
													FROM
														M_SHOUHIN_DATA AS M_ERROR_JAN_1
													WHERE SKUコード LIKE '3%'
										))
								AND (NOT EXISTS(
											SELECT
												売上伝票日付,
												発生日,
												勘定コード,
												店舗コード,
												SKUコード,
												買取受託,
												売上数,
												売上売価,
												売上値引,
												トランゼクションID,
												税コード,
												登録日時
											FROM
												DBO.T_DAY_TEN_ERROR_JAN_NEBIKI AS ERR
											WHERE
												(
													売上伝票日付 = TUM1.売上伝票日付
												)
											AND (
													店舗コード = TUM1.店舗コード
												)
											AND (
													SKUコード = TUM1.SKUコード
												)
											AND (
													勘定コード = '802'
												)
										))
							) AS MD
							LEFT OUTER JOIN
								DBO.M_BAIKA_RIREKI AS MB
							ON  MB.ＳＫＵコード = MD.品目コード
							AND MD.伝票日付 BETWEEN MB.有効開始日 AND MB.有効終了日
						GROUP BY
							MD.伝票日付,
							MD.店舗コード,
							MD.品目コード,
							MD.税コード
						UNION ALL
						SELECT
							伝票日付,
							店舗コード,
							品目コード,
							SUM(売上数量) AS 売上数量,
							SUM(売上金額) AS 売上金額,
							SUM(返品数量) AS 返品数量,
							SUM(返品金額) AS 返品金額,
							0 AS 売上値引金額,
							'TC08' AS 税コード
						FROM
							(
								SELECT
									売上伝票日付 AS 伝票日付,
									店舗コード,
									SKUコード AS 品目コード,
									売上数 AS 売上数量,
									売上売価 AS 売上金額,
									0 AS 売上販売値引,
									0 AS 返品数量,
									0 AS 返品金額,
									0 AS 返品販売値引
								FROM
									DBO.T_DAY_TEN_ERROR_JAN_NEBIKI
								WHERE
									(
										勘定コード = '801'
									)
								AND (
										売上伝票日付 BETWEEN  ? AND ? 
									)
								UNION ALL
								SELECT
									売上伝票日付 AS 伝票日付,
									店舗コード,
									SKUコード AS 品目コード,
									0 AS 売上数量,
									0 AS 売上金額,
									0 AS 売上販売値引,
									売上数 AS 返品数量,
									売上売価 AS 返品金額,
									0 AS 返品販売値引
								FROM
									DBO.T_DAY_TEN_ERROR_JAN_NEBIKI AS T_DAY_TEN_ERROR_JAN_NEBIKI_1
								WHERE
									(
										勘定コード = '802'
									)
								AND (
										売上伝票日付  BETWEEN ? AND ? 
									)
							) AS ERRORJAN
						GROUP BY
							伝票日付,
							店舗コード,
							品目コード
						UNION ALL
						SELECT
							伝票日付,
							店舗コード,
							品目コード,
							売上数量,
							売上金額,
							返品数量,
							返品金額,
							売上値引 AS 売上値引金額,
							税コード
						FROM
							(
								SELECT
									MD.伝票日付,
									MD.店舗コード,
									MD.品目コード,
									SUM(MD.売上数量) AS 売上数量,
									SUM(MD.売上金額 + MD.売上割引) AS 売上金額,
									SUM(MD.返品数量) AS 返品数量,
									SUM(MD.返品金額 + MD.返品割引) AS 返品金額,
									SUM(MB.販売価格 * (MD.売上数量 - MD.返品数量) - ((MD.売上金額 + MD.売上割引) - (MD.返品金額 + MD.返品割引))) AS 売上値引,
									MD.税コード
								FROM
									(
										SELECT
											店舗コード,
											SKUコード AS 品目コード,
											売上伝票日付 AS 伝票日付,
											売上数 AS 売上数量,
											売上売価 AS 売上金額,
											売上割引,
											0 AS 返品数量,
											0 AS 返品金額,
											0 AS 返品割引,
											税コード
										FROM
											DBO.D_URIAGE_DENPYOU_MEISAI AS TUM1
										WHERE
											(
												売上伝票日付  BETWEEN  ? AND ? 
											)
										AND (
												勘定コード = '801'
											)
										AND (SKUコード NOT IN(
													SELECT
														SKUコード
													FROM
														M_SHOUHIN_DATA AS M_ERROR_JAN_1
													WHERE SKUコード LIKE '3%'
												))
										UNION ALL
										SELECT
											店舗コード,
											SKUコード AS 品目コード,
											売上伝票日付 AS 伝票日付,
											0 AS 売上数量,
											0 AS 売上金額,
											0 AS 売上割引,
											売上数 AS 返品数量,
											売上売価 AS 返品金額,
											売上割引 AS 返品割引,
											税コード
										FROM
											DBO.D_URIAGE_DENPYOU_MEISAI AS TUM1
										WHERE
											(
												売上伝票日付  BETWEEN  ? AND ? 
											)
										AND (
												勘定コード = '802'
											)
										AND (SKUコード NOT IN(
													SELECT
														SKUコード
													FROM
														M_SHOUHIN_DATA AS M_ERROR_JAN_1
													WHERE SKUコード LIKE '3%'
												))
									) AS MD
									LEFT OUTER JOIN
										DBO.M_BAIKA_RIREKI AS MB
									ON  MB.ＳＫＵコード = MD.品目コード
									AND MD.伝票日付 BETWEEN MB.有効開始日 AND MB.有効終了日
								GROUP BY
									MD.伝票日付,
									MD.店舗コード,
									MD.品目コード,
									MD.税コード
							) AS TEST
					) AS URIAGE
				GROUP BY
					伝票日付,
					店舗コード,
					品目コード
			) AS URIAGE_MEISAI
			LEFT OUTER JOIN
				(
					SELECT
						MST.SKUコード,
						MST.部門コード,
						JOKEN.仕入条件 AS 仕入条件区分,
						JOKEN.有効開始日,
						JOKEN.有効終了日
					FROM
						DBO.M_SHIIRE_JOUKEN_RIREKI AS JOKEN
						LEFT OUTER JOIN
							DBO.M_SHOUHIN_DATA AS MST
						ON  JOKEN.商品コード = MST.商品コード
				) AS JOKEN_1
			ON  URIAGE_MEISAI.SKUコード = JOKEN_1.SKUコード
			AND URIAGE_MEISAI.運用日付 BETWEEN JOKEN_1.有効開始日 AND JOKEN_1.有効終了日
			LEFT OUTER JOIN
				DBO.MI_SYS_CALENDAR_INFO AS CAL
			ON  URIAGE_MEISAI.運用日付 = CAL.カレンダー年月日
			WHERE URIAGE_MEISAI.SKUコード NOT IN ('2082100000010', '2088100000029','2081100000013',
		'2084100000014','2081100000013','2082100000010','2088100000029',
		'2081100000020','2081100000037','2081100000044','2083100000017')
		GROUP BY URIAGE_MEISAI.運用日付) AS D_URIAGE
		FULL JOIN
			(SELECT
			伝票日付,	
			SUM(売上値引金額) AS 売上値引金額
		FROM (
		SELECT
			MD.伝票日付,
			MD.店舗コード,
			MD.品目コード,
			SUM(MD.売上数) AS 売上数量,
			SUM(MD.売上金額) AS 売上金額,
			SUM(MD.返品数) AS 返品数量,
			SUM(MD.返品金額) AS 返品金額,
			SUM(ABS(MD.売上割引) - ABS(MD.返品割引)) AS 売上値引金額,
			'TC08' AS 税コード
		FROM
			(
				SELECT
					CASE
						WHEN URIAGE.店舗コード IS NULL THEN HENPIN.店舗コード
						ELSE URIAGE.店舗コード
					END AS 店舗コード,
					CASE
						WHEN URIAGE.SKUコード IS NULL THEN HENPIN.SKUコード
						ELSE URIAGE.SKUコード
					END AS 品目コード,
					CASE
						WHEN URIAGE.売上伝票日付 IS NULL THEN HENPIN.売上伝票日付
						ELSE URIAGE.売上伝票日付
					END AS 伝票日付,
					CASE
						WHEN URIAGE.売上数 IS NULL THEN 0
						ELSE URIAGE.売上数
					END AS 売上数,
					CASE
						WHEN URIAGE.売上売価 IS NULL THEN 0
						ELSE URIAGE.売上売価
					END AS 売上金額,
					CASE
						WHEN URIAGE.売上割引 IS NULL THEN 0
						ELSE URIAGE.売上割引
					END AS 売上割引,
					CASE
						WHEN HENPIN.売上数 IS NULL THEN 0
						ELSE HENPIN.売上数
					END AS 返品数,
					CASE
						WHEN HENPIN.売上売価 IS NULL THEN 0
						ELSE HENPIN.売上売価
					END AS 返品金額,
					CASE
						WHEN HENPIN.売上割引 IS NULL THEN 0
						ELSE HENPIN.売上割引
					END AS 返品割引
				FROM
					(
						SELECT
							店舗コード,
								SKUコード,
								売上伝票日付,
								売上売価,
								売上数,
								売上割引,
								勘定コード,
								税コード
						FROM
							D_URIAGE_DENPYOU_MEISAI AS TUM1
						WHERE
							(
								売上伝票日付 BETWEEN  ? AND ? 
							)
						AND (
								勘定コード  = '801'
							)
						AND SKUコード IN (
										SELECT
											SKUコード
										FROM
											M_SHOUHIN_DATA AS M_ERROR_JAN_1
										WHERE SKUコード LIKE '3%'
									)
					) AS URIAGE
					FULL OUTER JOIN
						(
							SELECT
								店舗コード,
								SKUコード,
								売上伝票日付,
								売上売価,
								売上数,
								売上割引,
								勘定コード,
								税コード
							FROM
								D_URIAGE_DENPYOU_MEISAI AS TUM1
							WHERE
								(
									売上伝票日付 BETWEEN  ? AND  ? 
								)
							AND (
									勘定コード = '802'
								)
							AND SKUコード IN (
										SELECT
											SKUコード
										FROM
											M_SHOUHIN_DATA AS M_ERROR_JAN_1
										WHERE SKUコード LIKE '3%'
									)
						) AS HENPIN
					ON  URIAGE.店舗コード = HENPIN.店舗コード
					AND URIAGE.売上伝票日付 = HENPIN.売上伝票日付
					AND URIAGE.SKUコード = HENPIN.SKUコード
					AND URIAGE.税コード = HENPIN.税コード
			) AS MD
			GROUP BY
				MD.伝票日付,
				MD.店舗コード,
				MD.品目コード
		UNION ALL
			SELECT
				伝票日付,
				店舗コード,
				SKUコード,
				SUM(売上数量) AS 売上数量,
				SUM(売上売価) AS 売上金額,
				SUM(返品数量) AS 返品数量,
				SUM(返品金額) AS 返品金額,
				SUM(売上販売値引 - 返品販売値引) AS 売上値引金額,
				'TC08' AS 税コード
			FROM (
			SELECT
				売上伝票日付 AS 伝票日付,
				店舗コード,
				SKUコード,
				売上数 AS 売上数量,
				売上売価,
				売上値引 AS 売上販売値引, 
				0 AS 返品数量,
				0 AS 返品金額,
				0 AS 返品販売値引
			FROM
				T_DAY_TEN_ERROR_JAN_NEBIKI WHERE 売上伝票日付 BETWEEN  ? AND ?  AND 勘定コード = '801'
			UNION ALL
			SELECT
				売上伝票日付 AS 伝票日付,
				店舗コード,
				SKUコード,
				0 AS 売上数量,
				0 AS 売上金額,
				0 AS 売上販売値引,
				売上数 AS 返品数量,
				売上売価 AS 返品金額,
				売上値引 AS 返品販売値引
			FROM
				T_DAY_TEN_ERROR_JAN_NEBIKI WHERE 売上伝票日付 BETWEEN  ? AND  ?   AND 勘定コード = '802'
		) ERRORJAN
		GROUP BY
			伝票日付,
			店舗コード,
			SKUコード ) ERRO_JAN_FINAL
		LEFT JOIN M_SHOUHIN_DATA MST
		ON MST.ＳＫＵコード = ERRO_JAN_FINAL.品目コード
		GROUP BY 伝票日付
		HAVING SUM(売上値引金額) != 0
		) AS ERROR_JAN
		ON ERROR_JAN.伝票日付 = D_URIAGE.運用日付
WHERE D_URIAGE.運用日付 BETWEEN ? AND ? 
	) AS URI
	FULL JOIN
	(
		SELECT
			売上計上日,
			SUM(純売上数) AS 純売上数,
			SUM(純売上額) AS 純売上額
		FROM T_DAY_TEN_REGI_SEISAN
		WHERE 売上計上日 BETWEEN ? AND ? 
		GROUP BY 売上計上日
	) AS REGI
ON URI.売上計上日 = REGI.売上計上日</property>
