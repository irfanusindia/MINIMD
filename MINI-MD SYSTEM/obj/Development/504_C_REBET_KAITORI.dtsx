            Encoding="UTF8"><![CDATA[#region Namespaces
using System.Collections.Generic;
using System.IO;
using System.Linq;
using System.Net;
using System.Text;
using System;
#endregion
namespace ST_d8917cfb519340c5a80c83a46a5d925c
{
    /// <summary>
    /// ScriptMain is the entry point class of the script.  Do not change the name, attributes,
    /// or parent of this class.
    /// </summary>
	[Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
	public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
	{
		public void Main()
		{
            // get all important data from variables
            string strLocalFile = Dts.Variables["User::filePath"].Value.ToString();
            string strRemotePath = Dts.Variables["User::str_ftp_path"].Value.ToString();
            string port = Dts.Variables["User::str_ftp_port"].Value.ToString();
            string server = Dts.Variables["User::str_ftp_server"].Value.ToString();
            string user = Dts.Variables["User::str_ftp_username"].Value.ToString();
            string password = Dts.Variables["User::str_ftp_password"].Value.ToString();
            string pattern = "*" + Dts.Variables["User::excelName"].Value.ToString() + "*.ZIP";

            //call ftp functions
            // make directory if not exists on ftp server
            _makeDirectory(strLocalFile, strRemotePath, port, server, user, password, pattern);
            
            //transfer files to ftp server
            _uploadFTPFile(strLocalFile, strRemotePath, port, server, user, password, pattern);

			Dts.TaskResult = (int)ScriptResults.Success;
		}

        #region Upload File

        /// <summary>
        ///     <para>Date : 2017-02-10 </para>
        ///     <para>Irfan Mansuri </para>
        ///     <para>Here from all local files, first we are extracting nendogetsudo code.</para>
        ///     <para>Files are uploaded on their respective folders using that nendogetsudo code.</para>
        ///  
        /// </summary>
        /// 
        /// <param name="strLocalFile">Local File Path</param>
        /// <param name="strRemoteFile">Remote File Path</param>
        /// <param name="port">Port number of FTP Server</param>
        /// <param name="server">Server's IP Address</param>
        /// <param name="user">Username of FTP Server</param>
        /// <param name="password">Password of User</param>
        /// <param name="fileExtension">File Extension</param>
        public void _uploadFTPFile(string strLocalFile, string strRemoteFile, string port, string server, string user, string password, string pattern)
        {
            //string strHost = "ftp://" + "" + user + ":" + password + "@" + server + ":" + port;
            string strHost = "ftp://" + "" + user + ":" + password + "@" + server;
            FtpWebRequest ftpRequest = null;
            FileStream fileStream = null;
            int intBufferLength = 2048;
            string strFileName = string.Empty;
            string strNendoGetsudo = string.Empty;
            //loop thru all local shiiresaki files.
            string[] strFtp_files_list = Directory.GetFiles(strLocalFile, pattern);
            try
            {
                foreach (string shiiresaki_file in strFtp_files_list)
                {

                    //get file name and nendoGetsudo from path.
                    strFileName = Path.GetFileName(shiiresaki_file); // get file name
                    byte[] theBytes = Encoding.GetEncoding("UTF-8").GetBytes(Path.GetFileName(shiiresaki_file));
                    strNendoGetsudo = Path.GetFileNameWithoutExtension(Encoding.GetEncoding("UTF-8").GetString(theBytes)).Split('_').Last().ToString().Replace(".CSV", ""); //get nendogetsudo code from filename

                    // transfer current files according to nendoGetsudo folders.
                    ftpRequest = (FtpWebRequest)FtpWebRequest.Create(strHost + strRemoteFile + "/" + strNendoGetsudo + "/" + Encoding.GetEncoding("UTF-8").GetString(theBytes));
                    ftpRequest.Credentials = new NetworkCredential(user, password);
                    ftpRequest.Method = WebRequestMethods.Ftp.UploadFile;
                    //ftpRequest.UseBinary = true;
                    ftpRequest.UsePassive = true;
                    ftpRequest.KeepAlive = true;
                    FileInfo fileInfo = new FileInfo(shiiresaki_file);
                    fileStream = fileInfo.OpenRead();
                    byte[] buffer = new byte[intBufferLength];
                    Stream uploadStream = ftpRequest.GetRequestStream();
                    int contentLength = fileStream.Read(buffer, 0, intBufferLength);
                    while (contentLength != 0)
                    {
                        uploadStream.Write(buffer, 0, contentLength);
                        contentLength = fileStream.Read(buffer, 0, intBufferLength);
                    }
                    uploadStream.Close();
                    fileStream.Close();
                }
            }
            #region Catch / Finally
            catch (Exception ex)
            {
                throw ex;
            }
            finally
            {
                strLocalFile = null;
                strRemoteFile = null;
                port = null;
                server = null;
                user = null;
                password = null;
                pattern = null;
                strHost = null;
                strFileName = null;
                strNendoGetsudo = null;
                strFtp_files_list = null;
                ftpRequest = null;
                fileStream = null;
            }
            #endregion
            
        }
        #endregion

        #region Make Directory
        /// <summary>
        ///     <para>Date : 2017-02-10 </para>
        ///     <para>Irfan Mansuri </para>
        ///     <para>From the list of files available in local, we are extracting nendoGetsudo code</para>
        ///     <para>Here we are making directories based on nendoGetsudo code</para>
        /// </summary>
        /// <param name="strLocalFile">Local File Path</param>
        /// <param name="strRemoteFile">Remote File Path</param>
        /// <param name="port">Port number of FTP Server</param>
        /// <param name="server">Server's IP Address</param>
        /// <param name="user">Username of FTP Server</param>
        /// <param name="password">Password of User</param>
        /// <param name="fileExtension">File Extension</param>
        public void _makeDirectory(string strLocalFile, string strRemoteFile, string port, string server, string user, string password, string pattern)
        {
            //string strHost = "ftp://" + "" + user + ":" + password + "@" + server + ":" + port;
            string strHost = "ftp://" + "" + user + ":" + password + "@" + server;
            FtpWebRequest ftpRequest = null;
            List<string> totalCode = new List<string>();
            string[] ftp_files_list = Directory.GetFiles(strLocalFile, pattern);
            try
            {
                foreach (string shiiresaki_file in ftp_files_list)
                {
                    string nendoGetsudo = Path.GetFileNameWithoutExtension(shiiresaki_file).Split('_').Last().ToString().Replace(".CSV", "");
                    totalCode.Add(nendoGetsudo);
                }
                string[] folderList = totalCode.Distinct().ToArray();
                foreach (string nendoGetsudo in folderList)
                {
                    ftpRequest = (FtpWebRequest)FtpWebRequest.Create(strHost + strRemoteFile + "/" + nendoGetsudo);
                    ftpRequest.Credentials = new NetworkCredential(user, password);
                    ftpRequest.Method = WebRequestMethods.Ftp.MakeDirectory; // Make directory if it is not on the ftp server.
                    try
                    {
                        var resp = (FtpWebResponse)ftpRequest.GetResponse();
                    }
                    catch (WebException ex)
                    {
                    }
                }
            }
            #region Catch / Finally 
            catch (Exception ex)
            {
                throw ex;
            }
            finally
            {
                strLocalFile = null;
                strRemoteFile = null;
                port = null;
                server = null;
                user = null;
                password = null;
                pattern = null;
                strHost = null;
                ftpRequest = null;
                totalCode = null;
                ftp_files_list = null;
                ftpRequest = null;
            }
            #endregion
        }

        #endregion

        #region ScriptResults declaration
        /// <summary>
        /// This enum provides a convenient shorthand within the scope of this class for setting the
        /// result of the script.
        /// 
        /// This code was generated automatically.
        /// </summary>
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

	}
}]]></ProjectItem>
