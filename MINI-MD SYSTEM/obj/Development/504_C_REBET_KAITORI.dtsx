          SQLTask:Connection="{2E9DC886-0B27-45E8-9805-52F118D3F98C}"
          SQLTask:SqlStatementSource="SELECT&#xA;&#x9;CALENDER.年度+CALENDER.月度 AS DISTNENGETSU,MIN(CALENDER.カレンダー年月日) AS MINDATE,MAX(CALENDER.カレンダー年月日) AS MAXDATE&#xA;FROM D_KAIKEI_DENPYOU_MEISAI AS KAIKEI&#xA;LEFT JOIN MI_SYS_CALENDAR_INFO AS CALENDER&#xA;&#x9;ON CALENDER.カレンダー年月日 = KAIKEI.会計伝票日付&#xA;WHERE KAIKEI.勘定コード='701'&#xA;AND KAIKEI.会計伝票日付 NOT BETWEEN (SELECT MIN(月度開始日) AS MINDATE FROM M_DENPYOU_SIME_MASTER WHERE 月度締日 IS NOT NULL)&#xA;AND (SELECT MAX(月度終了日) AS MAXDATE FROM M_DENPYOU_SIME_MASTER WHERE 月度締日 IS NOT NULL)&#xA;GROUP BY 年度,月度&#xA;ORDER BY 年度,月度"
