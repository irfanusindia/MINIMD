CONVERT(NVARCHAR(200),[会計伝票番号]) AS 伝票番号,
CONVERT(NVARCHAR(200),'95') AS 種別,
CONVERT(NVARCHAR(200),SUM(国内通貨額)) AS 金額,
CONVERT(NVARCHAR(200),SUM(税額)) AS 税額,
CONVERT(NVARCHAR(200),SUM(税額) + SUM(国内通貨額)) AS 合計,
CONVERT(NVARCHAR(200),テキスト) AS 摘要,
CONVERT(NVARCHAR(200),GYOTAI.業態区分) AS 業態区分
FROM D_KAIKEI_DENPYOU_MEISAI AS KAIKEI
LEFT JOIN M_BUMON_MASTER AS BUMON
	ON BUMON.部門コード = KAIKEI.部門コード
	AND BUMON.クラスコード = KAIKEI.クラスコード
LEFT JOIN M_GYOUTAI_MASTER AS GYOTAI
	ON GYOTAI.業態コード = BUMON.業態コード
WHERE KAIKEI.勘定コード='701'
AND KAIKEI.会計伝票日付 BETWEEN ? AND ?
 AND KAIKEI.仕入先コード= ?
GROUP BY 仕入先コード,会計伝票日付,会計伝票番号,テキスト,GYOTAI.業態区分</property>
