          <components>
            <component
              refId="Package\データ フロー タスク\OLE DB Source"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="OLE DB Source"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
  [Y_DO],[M_DO],[W_DO],[SIR_CD],[SIR_NM],
  CONVERT(VARCHAR(8), [EGY_YMD], 112) AS [EGY_YMD],
  [TNP_CD],[SB_NM],[TNP_NM],[DEN_NO],[BMN_CD],
  [J_HBN],[SKU_CD],[M_HBN],[SHOHN_NM],[SIR_SU],
  SUM(CASE WHEN [SIR_SU]= 0 THEN 0 ELSE [SIR_GK]/[SIR_SU] END) AS [SIR_GT],
  SUM([SIR_GK]) as [SIR_GK],
  0 AS [SHP_SU],
  0 AS [SHP_GT],
  0 AS [SHP_GK],
  SUM([SIRJ_SU]) as [SIRJ_SU],
  SUM(CASE WHEN [SIRJ_SU]= 0 THEN 0 ELSE [SIRJ_GK]/[SIRJ_SU] END) AS [SIRJ_GT],
  SUM([SIRJ_GK]) as [SIRJ_GK],
  SUM([SHPJ_SU]) as [SHPJ_SU],
  SUM(CASE WHEN [SHPJ_SU]=0 THEN 0 ELSE [SHPJ_GK]/[SHPJ_SU] END) AS [SHPJ_GT],
  SUM([SHPJ_GK]) as [SHPJ_GK],[CMP_NM]
FROM (	
		SELECT
		  [勘定コード],[仕入伝票日付],
		  CI.[年度] AS [Y_DO],CI.[月度] AS [M_DO],
		  CI.[週度] AS [W_DO],[BMN_CD],[J_HBN],
		  [M_HBN],[SHOHN_NM],[SIR_CD],[SIR_NM],
		  [EGY_YMD],[TNP_CD],[SB_NM],[TNP_NM],
		  [DEN_NO],[SKU_CD],[SIR_SU],[商品コード],[SIR_GK],
		  --[SHP_SU],[SHP_GT],[SHP_GK],
		  [SIRJ_SU],[SIRJ_GK],[SHPJ_SU],[SHPJ_GK],[CMP_NM]
		FROM (
			SELECT
			  [勘定コード],[仕入伝票日付],[BMN_CD],[J_HBN],
			  [M_HBN],[SHOHN_NM],[SIR_CD],[SIR_NM],
			  [EGY_YMD],[TNP_CD],[SB_NM],
			  TM.[店舗名称] AS [TNP_NM],
			  [DEN_NO],[SKU_CD],[SIR_SU],[商品コード],[SIR_GK],
			  --[SHP_SU],[SHP_GT],[SHP_GK],
			  [SIRJ_SU],[SIRJ_GK],[SHPJ_SU],[SHPJ_GK],[CMP_NM]
			FROM (
				SELECT
				  [勘定コード],[仕入伝票日付],[BMN_CD],[J_HBN],
				  [M_HBN],[SHOHN_NM],[SIR_CD],SM.[仕入先名] AS [SIR_NM],
				  [EGY_YMD],[TNP_CD],[SB_NM],[DEN_NO],
				  [SKU_CD],[SIR_SU],[商品コード],[SIR_GK],
				  --[SHP_SU],[SHP_GT],[SHP_GK],
				  [SIRJ_SU],[SIRJ_GK],[SHPJ_SU],[SHPJ_GK],[CMP_NM]
				FROM (
					SELECT
					  [勘定コード],[仕入伝票日付],[BMN_CD],[J_HBN],
					  [M_HBN],[SHOHN_NM],[SIR_CD],[EGY_YMD],
					  [TNP_CD],[SB_NM],[DEN_NO],[SKU_CD],
					  [SIR_SU],[商品コード],[SIR_GK],
					  --[SHP_SU],[SHP_GT],[SHP_GK],
					  [SIRJ_SU],[SIRJ_GK],[SHPJ_SU],[SHPJ_GK],
					  CASE
						WHEN CHKBUMON.業態コード = 01 THEN 'RO'
						WHEN CHKBUMON.業態コード = 02 THEN 'FR'
						WHEN CHKBUMON.業態コード = 03 THEN 'CH'
						WHEN CHKBUMON.業態コード = 04 THEN 'ND'
						WHEN CHKBUMON.業態コード = 05 THEN 'RG'
						WHEN CHKBUMON.業態コード = 06 THEN 'SP'
						WHEN CHKBUMON.業態コード = 80 THEN 'EC'
						WHEN CHKBUMON.業態コード = 99 THEN 'RO'
						ELSE ''
					  END AS [CMP_NM]
					FROM (
						  SELECT
							[勘定コード],[仕入伝票日付],SD.部門コード [BMN_CD],
							SD.商品コード [J_HBN],SD.メーカー品番 [M_HBN],
							SD.商品テキスト [SHOHN_NM],[SIR_CD],[EGY_YMD],
							[TNP_CD],[SB_NM],[DEN_NO],[SKU_CD],[SIR_SU],
							SD.[商品コード] AS [商品コード],[SIR_GK],
							--[SHP_SU],[SHP_GT],[SHP_GK],
							[SIRJ_SU],[SIRJ_GK],[SHPJ_SU],[SHPJ_GK]
						FROM (
							SELECT
								SDM.勘定コード AS [勘定コード],SDM.仕入伝票日付 [仕入伝票日付],
								SDM.仕入先コード AS [SIR_CD],SDM.仕入伝票日付 AS [EGY_YMD],
								SDM.店舗コード AS [TNP_CD],
								CASE
									WHEN 勘定コード = 174 THEN '受託へ振替'
									WHEN 勘定コード = 102 THEN '受託仕入'
									WHEN 勘定コード IN (963,964) THEN '受託仕入訂正'
									WHEN 勘定コード = 162 THEN '受託返品'
									WHEN 勘定コード IN (973,974) THEN '受託返品訂正'
									WHEN 勘定コード = 173 THEN '受託戻し'
									WHEN 勘定コード = 172 THEN '棚卸調整'
									WHEN 勘定コード = 171 THEN '店頭売上'
								ELSE ''
								END AS [SB_NM],
								SDM.仕入伝票番号 AS [DEN_NO],SDM.SKUコード AS [SKU_CD],
								CASE 
									WHEN 勘定コード IN (174,102,173,963,964) THEN 実績数
									ELSE 0
								END AS SIRJ_SU,
								CASE 
									WHEN 勘定コード IN (174,102,173,963,964) THEN 原価額
									ELSE 0
								END AS SIRJ_GT,
								CASE 
									WHEN 勘定コード IN (174,102,173,963,964) THEN 原価額
									ELSE 0
								END AS SIRJ_GK,
								CASE 
									WHEN 勘定コード IN (162,973,974) THEN 実績数
									ELSE 0
								END AS SHPJ_SU,
								CASE 
									WHEN 勘定コード IN (162,973,974) THEN 原価額
									ELSE 0
								END AS SHPJ_GT,
								CASE 
									WHEN 勘定コード IN (162,973,974) THEN 原価額
									ELSE 0
								END AS SHPJ_GK,
								CASE 
									WHEN 勘定コード IN (172,171) THEN 実績数
									ELSE 0
								END AS SIR_SU,
								CASE 
									WHEN 勘定コード IN (172,171) THEN 原価額
									ELSE 0
								END AS SIR_GT,
								CASE 
									WHEN 勘定コード IN (172,171) THEN 原価額
									ELSE 0
								END AS SIR_GK
							FROM [dbo].[D_SIIRE_DENPYOU_MEISAI] SDM 
							

							WHERE --SDM.仕入伝票日付 between '2016-09-21' AND '2016-11-11' 
							SDM.仕入伝票日付 BETWEEN 
											(SELECT MIN(STARTDATE) AS STARTDATE FROM (
												SELECT TOP 2 MIN(カレンダー年月日) AS STARTDATE,
													MAX(カレンダー年月日) AS ENDDATE
												FROM MI_SYS_CALENDAR_INFO
												WHERE カレンダー年月日 BETWEEN CONVERT(DATE,DATEADD(MONTH,-2, ? )) AND CONVERT(DATE,DATEADD(DAY,-1, ? ))
												GROUP BY 年度+月度,月度ＳＥＱ
												ORDER BY 年度+月度 DESC
												) AS OLD)
											AND ?  
							AND SDM.勘定コード IN ('174','102','963','964','162','973','974','173','172','171')
AND SDM.買取受託='2'
							) AS SDMCHECK
						LEFT JOIN [dbo].[M_SHOUHIN_DATA] SD
						ON SDMCHECK.[SKU_CD] = SD.ＳＫＵコード
						) AS SDMSD
					LEFT JOIN (SELECT DISTINCT 部門コード,業態コード FROM [dbo].[M_BUMON_MASTER] BM) AS CHKBUMON
					ON CHKBUMON.[部門コード] = SDMSD.[BMN_CD]) AS SDMSDBM
				LEFT JOIN [dbo].[M_SIIRESAKI_MASTER] SM
				ON SM.[仕入先コード] = SDMSDBM.[SIR_CD]
				) AS SDMSDBMSM
			LEFT JOIN [dbo].[M_TENPO_MASTER] TM
			ON TM.[店舗コード] = SDMSDBMSM.[TNP_CD]
			) AS SDMSDBMSMCI
		LEFT JOIN [dbo].[MI_SYS_CALENDAR_INFO] CI
		ON CI.[カレンダー年月日] = SDMSDBMSMCI.[仕入伝票日付]
		) AS SDMSDBMSMCIGR
	LEFT JOIN [dbo].[M_GENKA_RIREKI] GR
	ON GR.[ＳＫＵコード] = SDMSDBMSMCIGR.[SKU_CD] AND 
	SDMSDBMSMCIGR.仕入伝票日付 BETWEEN GR.有効開始日 AND GR.有効終了日
GROUP BY
	 [Y_DO],[M_DO],[W_DO],[SIR_CD],[SIR_NM],
	 [EGY_YMD],[TNP_CD],[SB_NM],[TNP_NM],
	 [DEN_NO],[BMN_CD],[J_HBN],[SKU_CD],
	 [M_HBN],[SHOHN_NM],[SIR_SU],[CMP_NM]</property>
                <property
                  dataType="System.String"
                  description="The variable that contains the SQL command to be executed."
                  name="SqlCommandVariable"></property>
