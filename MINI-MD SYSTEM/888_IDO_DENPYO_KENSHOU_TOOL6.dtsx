              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	ISNULL(MEISAI.移動伝票日付,HDR.移動伝票日付) AS 伝票日付,
	ISNULL(MEISAI.移動績数,0) AS MS移動績数,
	ISNULL(MEISAI.在庫移動原価,0) AS MS在庫移動原価,
	ISNULL(MEISAI.在庫移動売価,0) AS MS伝票原価,
	ISNULL(MEISAI.移動績数,0) - ISNULL(HDR.移動績数,0) AS 移動績数差,
	ISNULL(MEISAI.在庫移動原価,0) - ISNULL(HDR.在庫移動原価,0) AS 在庫移動原価差,
	ISNULL(MEISAI.在庫移動売価,0) - ISNULL(HDR.在庫移動売価,0) AS 在庫移動売価差,
	ISNULL(HDR.移動績数,0) AS HDR移動績数,
	ISNULL(HDR.在庫移動原価,0) AS HDR在庫移動原価,
	ISNULL(HDR.在庫移動売価,0) AS HDR在庫移動売価
FROM (
		SELECT
			移動伝票日付,
			SUM(在庫移動数) AS 移動績数,
			SUM(在庫移動原価) AS 在庫移動原価,
			SUM(在庫移動売価) AS 在庫移動売価
		FROM D_ZAIKO_IDO_DENPYOU_MEISAI
		WHERE 移動伝票日付 BETWEEN ? AND  ? 
		GROUP BY 移動伝票日付
	) AS MEISAI
FULL JOIN (
		SELECT
			移動伝票日付,
			SUM(在庫移動数) AS 移動績数,
			SUM(在庫移動原価) AS 在庫移動原価,
			SUM(在庫移動売価) AS 在庫移動売価
		FROM D_ZAIKO_IDO_DENPYOU_HEADER
		WHERE 移動伝票日付 BETWEEN ? AND ? 
		GROUP BY 移動伝票日付
		) AS HDR
ON MEISAI.移動伝票日付 = HDR.移動伝票日付
ORDER BY MEISAI.移動伝票日付,HDR.移動伝票日付</property>
