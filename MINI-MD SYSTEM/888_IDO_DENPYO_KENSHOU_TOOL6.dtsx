                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	ISNULL(MEISAI.移動伝票日付,IDO.移動日付) AS 伝票日付,
	ISNULL(MEISAI.在庫移動数,0) AS 在庫移動数,
	ISNULL(MEISAI.在庫移動原価,0) AS 伝票原価,
	ISNULL(MEISAI.在庫移動売価,0) AS 伝票売価,
	ISNULL(MEISAI.在庫移動数,0) - ISNULL(IDO.移動数,0) AS 伝票点数差,
	ISNULL(MEISAI.在庫移動原価,0) - ISNULL(IDO.移動原価,0) AS 伝票原価差,
	ISNULL(MEISAI.在庫移動売価,0) - ISNULL(IDO.移動売価,0) AS 伝票売価差,
	ISNULL(IDO.移動数,0) AS 移動数,
	ISNULL(IDO.移動原価,0) AS 移動原価,
	ISNULL(IDO.移動売価,0) AS 移動売価
FROM (
	SELECT
		移動伝票日付,
		SUM(在庫移動数) AS 在庫移動数,
		SUM(在庫移動原価) AS 在庫移動原価,
		SUM(在庫移動売価) AS 在庫移動売価
	FROM D_ZAIKO_IDO_DENPYOU_MEISAI
	WHERE 移動伝票日付 BETWEEN ? AND  ? 
	GROUP BY 移動伝票日付
	) AS MEISAI
FULL JOIN (
		SELECT
			ISNULL(ISNULL(ISNULL(SHUKA.出荷日付,SHUYAKU.集約実績日),TENKAN.店間移動日),SOKOKAN.倉庫間移動日) AS 移動日付,
			ISNULL(SHUKA.出荷実績数,0) + ISNULL(SHUYAKU.集約実績数,0) + ISNULL(TENKAN.移動数,0) + ISNULL(SOKOKAN.移動数,0) AS 移動数,
			ISNULL(SHUKA.出荷原価額,0) + ISNULL(SHUYAKU.集約原価額,0) + ISNULL(TENKAN.移動原価額,0) + ISNULL(SOKOKAN.移動原価額,0) AS 移動原価,
			ISNULL(SHUKA.出荷売価額,0) + ISNULL(SHUYAKU.集約売価額,0) + ISNULL(TENKAN.移動売価額,0) + ISNULL(SOKOKAN.移動売価額,0) AS 移動売価
		FROM
			(
				SELECT 出荷日付,SUM(出荷実績数) AS 出荷実績数,
					SUM(出荷原価額) AS 出荷原価額,
					SUM(出荷売価額) AS 出荷売価額
				FROM T_DAY_TEN_SHUKKA_JISSEKI
				WHERE 出荷日付 BETWEEN ? AND  ? 
				GROUP BY 出荷日付
			) AS SHUKA
		FULL JOIN 
			(
				SELECT 集約実績日,SUM(集約実績数) AS 集約実績数,
					SUM(集約原価額) AS 集約原価額,
					SUM(集約売価額) AS 集約売価額
				FROM T_DAY_TEN_SHUUYAKU_JISSEKI
				WHERE 集約実績日 BETWEEN ? AND ? 
				GROUP BY 集約実績日
			) AS SHUYAKU
		ON SHUKA.出荷日付 = SHUYAKU.集約実績日
		FULL JOIN 
			(
				SELECT 店間移動日,SUM(移動数) AS 移動数,
					SUM(移動原価額) AS 移動原価額,
					SUM(移動売価額) AS 移動売価額
				FROM T_DAY_TENKAN_IDO_DATA
				WHERE 店間移動日 BETWEEN ? AND ? 
				GROUP BY 店間移動日
			) AS TENKAN
		ON SHUKA.出荷日付 = TENKAN.店間移動日
		FULL JOIN 
			(
				SELECT 倉庫間移動日,SUM(移動数) AS 移動数,
					SUM(移動原価額) AS 移動原価額,
					SUM(移動売価額) AS 移動売価額
				FROM T_DAY_SOUKOKAN_IDO_DATA
				WHERE 倉庫間移動日 BETWEEN ? AND ? 
				GROUP BY 倉庫間移動日
			) AS SOKOKAN
		ON SHUKA.出荷日付 = SOKOKAN.倉庫間移動日
		) AS IDO
ON MEISAI.移動伝票日付 = IDO.移動日付
ORDER BY MEISAI.移動伝票日付,IDO.移動日付</property>
