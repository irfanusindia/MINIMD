    /// </summary>
	[Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
	public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
	{
		/// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
		public void Main()
		{
			// TODO: Add your code here
            //User::mailFromId,User::mailFromName,User::mailServer,User::outputFolder
            string strMailFromId = Dts.Variables["User::mailFromId"].Value.ToString();
            string strMailFromName = Dts.Variables["User::mailFromName"].Value.ToString();
            string strMailServer = Dts.Variables["User::mailServer"].Value.ToString();
            string strmeisanFilePath = Dts.Variables["User::meisanFilePath"].Value.ToString();
            string strhdrFilePath = Dts.Variables["User::hdrFilePath"].Value.ToString();
            string strMailsReceivers = Dts.Variables["User::mailReceiver"].Value.ToString();
            try
            {
                _sendMail(strMailServer, strMailFromId, strMailFromName, strMailsReceivers, strmeisanFilePath, strhdrFilePath, DateTime.Now);
            }
            catch (Exception ex)
            {
                throw ex;
            }
            finally
            {
                strMailFromId = null;
                strMailFromName = null;
                strMailServer = null;
                strmeisanFilePath = null;
                strhdrFilePath = null;
                strMailsReceivers = null;
            }
			Dts.TaskResult = (int)ScriptResults.Success;
		}

        #region "Send Mail"
        /// <summary>
        /// Created Date: 2017/04/14
        /// Created By: Irfan Mansuri
        /// </summary>
        /// <param name="strMailServer"></param>
        /// <param name="strMailFromId"></param>
        /// <param name="strMailFromName"></param>
        /// <param name="strMailReceivers"></param>
        /// <param name="strMeisaiFile"></param>
        /// <param name="dtToday"></param>
        private void _sendMail(string strMailServer, string strMailFromId, string strMailFromName, string strMailReceivers, string strMeisaiFile, string strHeaderFile, DateTime dtToday)
        {
            MailAddress mailFrom;
            MailMessage mailMsg;
            string strTo = strMailReceivers;
            StringBuilder strBuildMailBody = new StringBuilder();
            try
            {
                #region Create Mail Body And Send Mail
                SmtpClient emailClient = new SmtpClient(strMailServer);
                emailClient.Port = 587;
                emailClient.Credentials = new NetworkCredential(strMailFromId, "righton0123");
                emailClient.DeliveryMethod = SmtpDeliveryMethod.Network;
                emailClient.EnableSsl = true;
                mailFrom = new MailAddress(strMailFromId, strMailFromName);
                mailMsg = new MailMessage();
                mailMsg.From = mailFrom;
                strBuildMailBody.Clear();
                //create Log file only if any error occured.
                if (File.Exists(strMeisaiFile))
                {
                    //Attach the created file
                    mailMsg.Attachments.Add(new Attachment(strMeisaiFile));
                }
                if (File.Exists(strHeaderFile))
                {
                    //Attach the created file
                    mailMsg.Attachments.Add(new Attachment(strHeaderFile));
                }
                //Start: MailContent
                strBuildMailBody.Clear();
                strBuildMailBody.Append("本メールはミニMDシステムからのお知らせです。<br /> <br />");
                strBuildMailBody.Append("締めなかった月度の検証レーポトを添付ファイルで送り致します。<br /> <br />");
                strBuildMailBody.Append("MINIMDで<b><i> 在庫移動トランジェくションと伝票明細-ヘーダの </i></b>データ検証。<br /> <br />");
                strBuildMailBody.Append("添付ファイルをご確認お願いいたします。<br /> <br />");
                strBuildMailBody.Append("以上です、<br />");
                strBuildMailBody.Append("宜しくお願い致します。<br />");
                strBuildMailBody.Append("<br /> <br />");
                //End: MailContent
                mailMsg.To.Add(strTo);
                mailMsg.Body = strBuildMailBody.ToString();

                mailMsg.BodyEncoding = Encoding.UTF8;
                mailMsg.Subject = "『MINIMD』の在庫移動実績と移動伝票検証_" + dtToday.ToString("yyyyMMdd");
                mailMsg.SubjectEncoding = Encoding.UTF8;
                mailMsg.IsBodyHtml = true;
                emailClient.Send(mailMsg);
                #endregion
            }
            catch (Exception ex)
            {
                throw ex;
            }
            finally 
            {
                strMailFromId = null;
                strMailFromName = null;
                strMailReceivers = null;
                strMailServer = null;
                mailFrom = null;
                mailMsg = null;
                strTo = null;
                strMeisaiFile = null;
                strHeaderFile = null;
            }
        }
        #endregion
        #region ScriptResults declaration
        /// <summary>
        /// This enum provides a convenient shorthand within the scope of this class for setting the
        /// result of the script.
        /// 
        /// This code was generated automatically.
        /// </summary>
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

	}
}]]></ProjectItem>
