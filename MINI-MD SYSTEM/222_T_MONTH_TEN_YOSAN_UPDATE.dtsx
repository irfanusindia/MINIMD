),&#xA;(T.年度 + '011',T.在庫原価6月度 - T.在庫原価7月度 ,'8294300'),&#xA;(T.年度 + '012',T.在庫原価7月度 - T.在庫原価8月度 ,'8294300'),&#xA;(T.年度 + '001',T.期首在庫売価, '8114000'),&#xA;(T.年度 + '002',0,'8114000'),&#xA;(T.年度 + '003',0,'8114000'),&#xA;(T.年度 + '004',0,'8114000'),&#xA;(T.年度 + '005',0,'8114000'),&#xA;(T.年度 + '006',0,'8114000'),&#xA;(T.年度 + '007',0,'8114000'),&#xA;(T.年度 + '008',0,'8114000'),&#xA;(T.年度 + '009',0,'8114000'),&#xA;(T.年度 + '010',0,'8114000'),&#xA;(T.年度 + '011',0,'8114000'),&#xA;(T.年度 + '012',0,'8114000')&#xA;) AS X (YEARTIME,BALANCE,KANJO)&#xA;ORDER BY X.YEARTIME;&#xA;&#xA;GO&#xA;&#xA;DELETE MAIN&#xA;FROM &#xA;IF_KAIKEI_YOSAN_DATA AS MAIN&#xA;LEFT JOIN &#xA;IF_KAIKEI_YOSAN_DATA_TEMP AS TEMP&#xA;ON &#xA;LEFT(TEMP.VERSION_CODE,4) = LEFT(MAIN.VERSION_CODE,4)&#xA;GO&#xA;&#xA;INSERT INTO IF_KAIKEI_YOSAN_DATA&#xA;SELECT * FROM IF_KAIKEI_YOSAN_DATA_TEMP" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
