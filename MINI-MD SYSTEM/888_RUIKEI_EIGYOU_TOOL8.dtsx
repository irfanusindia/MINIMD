                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	ISNULL(COMPNY.IF_SYSTEM_FUNC_ID,SIIRE.ID) AS CMP_ID,
	ISNULL(COMPNY.DTL_AMOUNT,0) AS IF税抜き金額,
	ISNULL(COMPNY.DTL_AMOUNT,0) - ISNULL(SIIRE.税抜き金額,0) AS 税抜き金額差,
	ISNULL(SIIRE.税抜き金額,0) AS SIIRE税抜き金額
 FROM (
		SELECT
			IF_SYSTEM_FUNC_ID,SUM(DTL_AMOUNT) AS DTL_AMOUNT
		FROM IF_KAIKEI_SIIRE_DATA_KAITORI
		WHERE HD_STOCK_DATE BETWEEN  ?  AND  ? 
		GROUP BY IF_SYSTEM_FUNC_ID
		UNION ALL
		SELECT IF_SYSTEM_FUNC_ID,SUM(DTL_AMOUNT) AS DTL_AMOUNT
		FROM IF_KAIKEI_SIIRE_DATA_JUTAKU
		WHERE HD_STOCK_DATE BETWEEN  ?  AND  ? 
		GROUP BY IF_SYSTEM_FUNC_ID
	) AS COMPNY FULL OUTER JOIN
(
 SELECT 'MD1' AS ID,SUM(原価額) AS 税抜き金額
 FROM D_SIIRE_DENPYOU_MEISAI
 WHERE 仕入伝票日付 BETWEEN  ?  AND  ? 
	AND 勘定コード IN ('101','961','962') AND 店舗コード NOT IN ('7802')
 UNION ALL
 SELECT 'MD2' AS ID,SUM((-1)*原価額) AS 税抜き金額
 FROM D_SIIRE_DENPYOU_MEISAI
 WHERE 仕入伝票日付 BETWEEN  ?  AND   ?  
	AND 勘定コード IN ('161','972','971') AND 店舗コード NOT IN ('7802')
  UNION ALL
 SELECT 'MD5' AS ID,SUM(原価額) AS 税抜き金額
 FROM D_SIIRE_DENPYOU_MEISAI
 WHERE 仕入伝票日付 BETWEEN  ?  AND  ?  
	AND 勘定コード='171' AND 店舗コード NOT IN ('7802')
 UNION ALL
 SELECT 'MD15' AS ID,SUM(原価額) AS 税抜き金額
 FROM D_SIIRE_DENPYOU_MEISAI
 WHERE 仕入伝票日付 BETWEEN  ?  AND  ?  
	AND 勘定コード IN ('101','961','962') AND 店舗コード IN ('7802') AND 仕入先コード!='199999'
 UNION ALL
 SELECT 'MD10' AS IF_SYSTEM_FUNC_ID,SUM(原価額) AS 原価額
 FROM D_SIIRE_DENPYOU_MEISAI
 WHERE 勘定コード IN ('172') AND 発生日 BETWEEN  ?  AND  ?  AND 実績数 &gt;= 0
	AND 店舗コード NOT IN ('7802')
 UNION ALL
 SELECT 'MD11' AS IF_SYSTEM_FUNC_ID,SUM(原価額) AS 原価額
 FROM D_SIIRE_DENPYOU_MEISAI
 WHERE 勘定コード IN ('172') AND 発生日 BETWEEN  ?  AND  ?  AND 実績数 &lt; 0
	AND 店舗コード NOT IN ('7802')
 UNION ALL
 SELECT 'MD14' AS ID,SUM(国内通貨額) AS 税抜き金額 FROM D_KAIKEI_DENPYOU_MEISAI
 WHERE 会計伝票日付 BETWEEN  ?  AND  ?  
	AND 店舗コード NOT IN ('7802')  AND 勘定コード='701'
 UNION ALL
 SELECT ID,税抜き金額 FROM 
 (SELECT 'MD12' AS ID,SUM(金額) AS 税抜き金額 FROM 
	  (
		SELECT SUM(金額)*(-1) AS 金額
		FROM T_TAG_DAI_SOUSAI_DATA
		WHERE 日付 BETWEEN  ?  AND  ?  
	  UNION ALL
		SELECT SUM(DTL_AMOUNT) AS 金額
		FROM D_KAIKEI_CENTERFEE_DATA_REPORT_COMPANY_IFS
		WHERE HD_STOCK_DATE =  ? 
	  ) AS MD
 ) AS MD12
) AS SIIRE
ON COMPNY.IF_SYSTEM_FUNC_ID = SIIRE.ID</property>
