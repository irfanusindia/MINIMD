#endregion

namespace ST_1c9583bc846f427aa13629fc520dfad8
{
    /// <summary>
    /// ScriptMain is the entry point class of the script.  Do not change the name, attributes,
    /// or parent of this class.
    /// </summary>
	[Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
	public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
	{
		public void Main()
		{
            //System::ErrorDescription,
            //User::connectionString,
            //User::currentDate,
            //User::errFolderPath,
            //System::PackageName
            DateTime dtCurDate = Convert.ToDateTime(Dts.Variables["User::currentDate"].Value);
            string strErrorMessage = Dts.Variables["System::ErrorDescription"].Value.ToString();    //Old Message which came from last Throwner package.
            string strErrfolderPath = Dts.Variables["User::errFolderPath"].Value.ToString();
            StringBuilder strBuildQry = new StringBuilder();
            try
            {
                //全体パッケージでエラーの場合、エラーのログする。
                strBuildQry.Clear();
                strBuildQry.Append(" INSERT INTO KANSHI_PACKAGE_ERROR ");
                strBuildQry.Append(" (パッケージコード,データ日付,エラー日時,エラー内容,エラーフラグ) ");
                strBuildQry.Append(" VALUES('M999','" + dtCurDate + "','" + DateTime.Now + "','" + strErrorMessage + "','2');");
                _updateInsert(strBuildQry.ToString());  //Call Insert Data function
                _logging(strErrfolderPath, dtCurDate, strErrorMessage, string.Empty);    //Write Log in Text
                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception ex)
            {
                //Text Log Write
                _logging(strErrfolderPath, dtCurDate, strErrorMessage, ex.Message);
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
            finally
            {
                strErrfolderPath = null;
                strErrorMessage = null;
                strBuildQry = null;
            }
		}

        #region "Update/Insert Query in Db"
        protected void _updateInsert(string strQuery)
        {
            string strConnection = Dts.Variables["User::connectionString"].Value.ToString();
            StringBuilder strERRQuery = new StringBuilder();
            SqlConnection myConnection = new SqlConnection(strConnection);
            SqlCommand myCommand;
            try
            {
                myCommand = new SqlCommand(strQuery, myConnection);
                if (myConnection.State == ConnectionState.Closed)
                {
                    myConnection.Open();
                }
                myCommand.ExecuteNonQuery();
            }
            #region CATCH/FINALLY
            catch (Exception)
            {
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
            finally
            {
                if (myConnection.State == ConnectionState.Open)
                {
                    myConnection.Close();
                }
                strERRQuery = null;
                myConnection = null;
                myCommand = null;
            }
            #endregion
        }
        #endregion

        #region "Write Logging in Text"
        private void _logging(string strLogFolderPath, DateTime dtCurDate, string strErrorMsg, string strExceptionMessage)
        {
            string strLogFilePath = strLogFolderPath + @"\PACKAGE_" + Dts.Variables["System::PackageName"].Value.ToString() + "_" + DateTime.Now.ToString("yyyyMMdd") + ".log";
            StringBuilder strBuildLog = new StringBuilder();
            StreamWriter strmWrite = new StreamWriter(strLogFilePath, true);
            if (File.Exists(strLogFilePath))
            {
                strBuildLog.Clear();
                strBuildLog.Append(DateTime.Now.ToString());
                strBuildLog.Append("\t");
                strBuildLog.Append(strErrorMsg);
                if (!string.IsNullOrEmpty(strExceptionMessage))
                {
                    strBuildLog.Append("\n");
                    strBuildLog.Append(DateTime.Now.ToString());
                    strBuildLog.Append("\t");
                    strBuildLog.Append(strExceptionMessage);
                }
                strmWrite.WriteLine(strBuildLog.ToString());
            }
            else
            {
                File.Create(strLogFilePath).Close();
                strBuildLog.Clear();
                strBuildLog.Append(DateTime.Now.ToString());
                strBuildLog.Append("\t");
                strBuildLog.Append(strErrorMsg);
                if (!string.IsNullOrEmpty(strExceptionMessage))
                {
                    strBuildLog.Append("\n");
                    strBuildLog.Append(DateTime.Now.ToString());
                    strBuildLog.Append("\t");
                    strBuildLog.Append(strExceptionMessage);
                }
                strmWrite.WriteLine(strBuildLog.ToString());
            }
            #region reset vars
            if (strmWrite != null)
            {
                strmWrite.Flush();
                strmWrite.Close();
                strmWrite = null;
            }
            strLogFilePath = null;
            strBuildLog = null;
            strErrorMsg = null;
            strExceptionMessage = null;
            #endregion
        }
        #endregion

        #region ScriptResults declaration
        /// <summary>
        /// This enum provides a convenient shorthand within the scope of this class for setting the
        /// result of the script.
