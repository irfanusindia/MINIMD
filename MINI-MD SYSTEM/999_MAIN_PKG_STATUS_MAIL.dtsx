            <component
              refId="Package\全体パッケージステータス更新\GETパッケージステータス"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="GETパッケージステータス"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT MST.パッケージコード AS パッケージコード,
	MST.パッケージ名 AS パッケージ名,
    CONVERT(DATE,CAST( ? AS VARCHAR)) AS データ日付,
	CASE
		WHEN MST.データ検証 = '0' THEN '―'
		WHEN MST.データ検証 = '1' AND JUSHIN_ERR.jshnChkErrCnt &gt; 0 THEN '×'
		WHEN MST.データ検証 = '1' AND ISNULL(JUSHIN_ERR.jshnChkErrCnt,0) = 0 THEN '●'
	END AS 受信チェック,
	CASE WHEN TRN.実行ステータス != '未' THEN
			CASE
				WHEN MST.業務チェック = 'FALSE' THEN '―'
				WHEN MST.業務チェック = 'TRUE' AND GYOMCHECK_ERR.gyChkErrCnt &gt; 0 THEN '×'
				WHEN MST.業務チェック = 'TRUE' AND ISNULL(GYOMCHECK_ERR.gyChkErrCnt,0) = 0 THEN '●'
			END 
		ELSE TRN.実行ステータス
	END AS 業務チェック,
	CASE 
		WHEN TRN.実行ステータス = '済' THEN '●'
		WHEN TRN.実行ステータス = 'エラー' THEN '×'
	ELSE TRN.実行ステータス END AS 業務パッケージ,
	CASE 
		WHEN TRN.FTPプットステータス='済' THEN '●'
		WHEN TRN.FTPプットステータス='エラー' THEN '×' 
                                     WHEN TRN.FTPプットステータス='未' THEN 'ー'
	ELSE  TRN.FTPプットステータス
	END AS FTPプットステータス
FROM 
	KANSHI_PKG_MST_DETAILS AS MST
LEFT JOIN 
	KANSHI_PKG_TRN_DETAILS AS TRN
ON MST.パッケージコード=TRN.パッケージコード AND 
	MST.削除フラグ=TRN.削除フラグ AND 
	MST.削除フラグ='FALSE'
LEFT JOIN 
	(
		SELECT パッケージコード,COUNT(パッケージコード) as jshnChkErrCnt FROM KANSHI_PACKAGE_ERROR_STATUS WHERE データ日付= ? AND ステータス='NG' group by パッケージコード
	) as JUSHIN_ERR
ON
	MST.パッケージコード = JUSHIN_ERR.パッケージコード
LEFT JOIN
	(
		SELECT パッケージコード,COUNT(パッケージコード)  as gyChkErrCnt FROM KANSHI_PACKAGE_ERROR WHERE データ日付= ?  group by パッケージコード
	) as GYOMCHECK_ERR
ON
	'G' + MST.パッケージコード = GYOMCHECK_ERR.パッケージコード
WHERE TRN.削除フラグ='FALSE'
ORDER BY MST.パッケージコード;</property>
